#. type: Plain text
#: opensuse-leap-15-5
#, no-wrap
msgid ""
"static void\n"
"test_kcmp(char *msg, id_t pid1, pid_t pid2, int fd_a, int fd_b)\n"
"{\n"
"    printf(\"\\et%s\\en\", msg);\n"
"    printf(\"\\et\\etkcmp(%ld, %ld, KCMP_FILE, %d, %d) ==E<gt> %s\\en\",\n"
"            (long) pid1, (long) pid2, fd_a, fd_b,\n"
"            (kcmp(pid1, pid2, KCMP_FILE, fd_a, fd_b) == 0) ?\n"
"                        \"same\" : \"different\");\n"
"}\n"
msgstr ""


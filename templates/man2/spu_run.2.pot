#: mageia-cauldron opensuse-leap-15-5 opensuse-tumbleweed
msgid "spu_run - execute an SPU context"
msgstr ""

#. type: SH
#: archlinux
#, no-wrap
msgid "LIBRARY"
msgstr ""

#. type: Plain text
#: archlinux
msgid "Standard C library (I<libc>, I<-lc>)"
msgstr ""

#. type: SH

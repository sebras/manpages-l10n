#. type: SH
#: archlinux
#, no-wrap
msgid "EXAMPLES"
msgstr ""

#. type: Plain text
#: archlinux
msgid ""
"The program below is a port of the one found in B<strtok>(3), which, "
"however, doesn't discard multiple delimiters or empty tokens:"
msgstr ""

#. type: Plain text
#: archlinux
#, no-wrap
msgid ""
"$B< ./a.out \\(aqa/bbb///cc;xxx:yyy:\\(aq \\(aq:;\\(aq \\(aq/\\(aq>\n"
"1: a/bbb///cc\n"
"         --E<gt> a\n"
"         --E<gt> bbb\n"
"         --E<gt>\n"
"         --E<gt>\n"
"         --E<gt> cc\n"
"2: xxx\n"
"         --E<gt> xxx\n"
"3: yyy\n"
"         --E<gt> yyy\n"
"4:\n"
"         --E<gt>\n"
msgstr ""

#. type: SS
#: archlinux
#, no-wrap
msgid "Program source"
msgstr ""

#. type: Plain text
#: archlinux
#, no-wrap
msgid ""
"#include E<lt>stdio.hE<gt>\n"
"#include E<lt>stdlib.hE<gt>\n"
"#include E<lt>string.hE<gt>\n"
msgstr ""

#. type: Plain text
#: archlinux
#, no-wrap
msgid ""
"int\n"
"main(int argc, char *argv[])\n"
"{\n"
"    char *token, *subtoken;\n"
msgstr ""

#. type: Plain text
#: archlinux
#, no-wrap
msgid ""
"    if (argc != 4) {\n"
"        fprintf(stderr, \"Usage: %s string delim subdelim\\en\", argv[0]);\n"
"        exit(EXIT_FAILURE);\n"
"    }\n"
msgstr ""

#. type: Plain text
#: archlinux
#, no-wrap
msgid ""
"    for (unsigned int j = 1; (token = strsep(&argv[1], argv[2])); j++) {\n"
"        printf(\"%u: %s\\en\", j, token);\n"
msgstr ""

#. type: Plain text
#: archlinux
#, no-wrap
msgid ""
"        while ((subtoken = strsep(&token, argv[3])))\n"
"            printf(\"\\et --E<gt> %s\\en\", subtoken);\n"
"    }\n"
msgstr ""

#. type: Plain text
#: archlinux
#, no-wrap
msgid ""
"    exit(EXIT_SUCCESS);\n"
"}\n"
msgstr ""


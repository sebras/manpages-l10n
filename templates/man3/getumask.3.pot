msgid "getumask - get file creation mask"
msgstr ""

#. type: SH
#: debian-bullseye debian-unstable opensuse-leap-15-4
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
#: debian-bullseye debian-unstable opensuse-leap-15-4
msgid "B<#define _GNU_SOURCE> /* See feature_test_macros(7) */"
msgstr ""

#. type: Plain text
#: debian-bullseye debian-unstable opensuse-leap-15-4
msgid "B<#include E<lt>sys/types.hE<gt>>"
msgstr ""

#. type: Plain text

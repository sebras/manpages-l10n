#, no-wrap
msgid ""
"    free(p);\n"
"    printf(\"%s(): returned from first free() call\\en\", __func__);\n"
msgstr ""

#. type: Plain text
#: archlinux debian-unstable fedora-rawhide mageia-cauldron
#, no-wrap
msgid ""
"    free(p);\n"
"    printf(\"%s(): returned from second free() call\\en\", __func__);\n"
msgstr ""

#. type: Plain text

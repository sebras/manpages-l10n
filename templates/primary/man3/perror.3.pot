msgid "NAME"
msgstr ""

#. type: Plain text
msgid "perror - print a system error message"
msgstr ""

#. type: SH
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
msgid "B<#include E<lt>stdio.hE<gt>>"
msgstr ""

#. type: Plain text
msgid "B<void perror(const char *>I<s>B<);>"
msgstr ""

#. type: Plain text
msgid "B<#include E<lt>errno.hE<gt>>"
msgstr ""

#. type: Plain text
msgid "B<const char * const >I<sys_errlist>B<[];>"
msgstr ""


#. type: SH
#, no-wrap
msgid "NAME"
msgstr ""

#. type: Plain text
msgid "chmod, fchmod, fchmodat - change permissions of a file"
msgstr ""

#. type: SH
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
#, no-wrap
msgid "B<#include E<lt>sys/stat.hE<gt>>\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid "B<int chmod(const char *>I<pathname>B<, mode_t >I<mode>B<);>\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid "B<int fchmod(int >I<fd>B<, mode_t >I<mode>B<);>\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
"B<#include E<lt>fcntl.hE<gt>>           /* Definition of AT_* constants */\n"
"B<#include E<lt>sys/stat.hE<gt>>\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
"B<int fchmodat(int >I<dirfd>B<, const char *>I<pathname>B<, mode_t "
">I<mode>B<, int >I<flags>B<);>\n"
msgstr ""

#. type: Plain text
msgid "Feature Test Macro Requirements for glibc (see B<feature_test_macros>(7)):"
msgstr ""


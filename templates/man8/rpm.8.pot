msgid "List capabilities enhanced by package(s)"
msgstr ""

#. type: TP
#: debian-bullseye opensuse-leap-15-5
#, no-wrap
msgid "B<--filesbypkg>"
msgstr ""

#. type: Plain text
#: debian-bullseye opensuse-leap-15-5
msgid "List all the files in each selected package."
msgstr ""

#. type: TP
#: debian-bullseye opensuse-leap-15-5
#, no-wrap
msgid "B<--filetriggers>"
msgstr ""


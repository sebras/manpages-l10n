#. type: TP
#: debian-bullseye opensuse-leap-15-5
#, no-wrap
msgid "B<-l, --list>"
msgstr ""

#. type: Plain text
#: debian-bullseye opensuse-leap-15-5
msgid "List files in package."
msgstr ""

#. type: TP
#: debian-bullseye opensuse-leap-15-5
#, no-wrap
msgid "B<--obsoletes>"
msgstr ""

#. type: Plain text
#: debian-bullseye opensuse-leap-15-5
msgid "List packages this package obsoletes."
msgstr ""

#. type: TP
#: debian-bullseye opensuse-leap-15-5
#, no-wrap
msgid "B<--provides>"
msgstr ""

#. type: Plain text
#: debian-bullseye opensuse-leap-15-5
msgid "List capabilities this package provides."
msgstr ""

#. type: TP
#: debian-bullseye opensuse-leap-15-5
#, no-wrap
msgid "B<--recommends>"
msgstr ""

#. type: Plain text
#: debian-bullseye opensuse-leap-15-5
msgid "List capabilities recommended by package(s)"
msgstr ""

#. type: TP
#: debian-bullseye opensuse-leap-15-5
#, no-wrap
msgid "B<-R, --requires>"

msgstr ""

#. type: TP
#: debian-bullseye opensuse-leap-15-4
#, no-wrap
msgid "B<--whatenhances >I<CAPABILITY>"
msgstr ""

#. type: Plain text
#: debian-bullseye opensuse-leap-15-4
msgid "Query all packages that enhance I<CAPABILITY>."
msgstr ""

#. type: SS
#: debian-bullseye debian-unstable opensuse-leap-15-4
#, no-wrap
msgid "PACKAGE QUERY OPTIONS:"
msgstr ""


msgid "Wed Feb 26 2014"
msgstr ""

#. type: SH
#: fedora-rawhide mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
#, no-wrap
msgid "NAME"
msgstr ""

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "B<grub-set-default> \\(em Set the default boot menu entry for GRUB."
msgstr ""

#. type: SH
#: fedora-rawhide mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "B<grub-set-default> [--boot-directory=I<DIR>] I<MENU_ENTRY>"
msgstr ""

#. type: SH
#: fedora-rawhide mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
#, no-wrap
msgid "DESCRIPTION"
msgstr ""

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid ""
"B<grub-set-default> sets the default boot menu entry for all subsequent "
"boots.  This command only works for GRUB configuration files created with "
"I<GRUB_DEFAULT=saved> in I</etc/default/grub>."
msgstr ""

#. type: SH
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "OPTIONS"
msgstr ""

#. type: TP
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "--boot-directory=I<DIR>"
msgstr ""

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid ""
"Find GRUB images under I<DIR/grub>.  The default value is I</boot>, "
"resulting in grub images being search for at I</boot/grub>."
msgstr ""

#. type: TP
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "I<MENU_ENTRY>"
msgstr ""

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "A number, a menu item title or a menu item identifier."
msgstr ""

#. type: SH
#: fedora-rawhide mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
#, no-wrap
msgid "SEE ALSO"
msgstr ""

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "B<info grub>"
msgstr ""


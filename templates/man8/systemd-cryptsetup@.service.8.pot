#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4
msgid "systemd-cryptsetup@\\&.service"
msgstr ""

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4
msgid "system-systemd\\ex2dcryptsetup\\&.slice"
msgstr ""

#. type: Plain text
#: archlinux fedora-36 fedora-rawhide mageia-cauldron opensuse-leap-15-4
msgid "/usr/lib/systemd/systemd-cryptsetup"
msgstr ""

#. type: SH

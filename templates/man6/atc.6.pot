#: opensuse-leap-15-4 opensuse-tumbleweed
#, no-wrap
msgid ""
"exit:\t\t( 12  0 x ) ( 29  0 z ) ( 29  7 a ) ( 29 17 a )\n"
"\t\t(  9 20 e ) (  0 13 d ) (  0  7 d ) (  0  0 c ) ;\n"
msgstr ""

#. type: Plain text
#: debian-bullseye debian-unstable fedora-rawhide mageia-cauldron
#: opensuse-leap-15-4 opensuse-tumbleweed
#, no-wrap
msgid "beacon:\t\t( 12  7 ) ( 12 17 ) ;\n"
msgstr ""

#. type: Plain text
#: debian-bullseye debian-unstable fedora-rawhide mageia-cauldron
#: opensuse-leap-15-4 opensuse-tumbleweed
#, no-wrap
msgid "airport:\t( 20 15 w ) ( 20 18 d ) ;\n"
msgstr ""

#. type: Plain text
#: debian-bullseye debian-unstable fedora-rawhide mageia-cauldron
#: opensuse-leap-15-4 opensuse-tumbleweed
#, no-wrap
msgid ""
"line:\t\t[ (  1  1 ) (  6  6 ) ]\n"
"\t\t[ ( 12  1 ) ( 12  6 ) ]\n"
"\t\t[ ( 13  7 ) ( 28  7 ) ]\n"
"\t\t[ ( 28  1 ) ( 13 16 ) ]\n"
"\t\t[ (  1 13 ) ( 11 13 ) ]\n"
"\t\t[ ( 12  8 ) ( 12 16 ) ]\n"
"\t\t[ ( 11 18 ) ( 10 19 ) ]\n"
"\t\t[ ( 13 17 ) ( 28 17 ) ]\n"
"\t\t[ (  1  7 ) ( 11  7 ) ] ;\n"
msgstr ""


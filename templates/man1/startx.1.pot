#: fedora-37 fedora-rawhide mageia-cauldron opensuse-leap-15-5
#: opensuse-tumbleweed
msgid "I</usr/lib64/sys.startxrc>"
msgstr ""

#. type: TP
#: opensuse-tumbleweed
#, no-wrap
msgid "I</usr/libexec/xinit/xinitrc>"
msgstr ""

#. type: Plain text

"/dev/hda4:  Linux/i386 ext2 filesystem\n"
"/dev/hda5:  Linux/i386 swap file\n"
"/dev/hda6:  Linux/i386 swap file\n"
"/dev/hda7:  Linux/i386 swap file\n"
"/dev/hda8:  Linux/i386 swap file\n"
"/dev/hda9:  empty\n"
"/dev/hda10: empty\n"

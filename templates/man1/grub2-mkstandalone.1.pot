#. type: SH
#: fedora-rawhide mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
#, no-wrap
msgid "NAME"
msgstr ""

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid ""
"B<grub-mkstandalone> \\(em Generate a standalone image in the selected "
"format."
msgstr ""

#. type: SH
#: fedora-rawhide mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "B<grub-mkstandalone> [-o | --output=I<FILE>] [-O | --format=I<FORMAT>]"
msgstr ""

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "[-C | --compression=(I<xz>|I<none>|I<auto>)]"
msgstr ""

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "[--modules=I<MODULES>] [--install-modules=I<MODULES>]"
msgstr ""

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "[--themes=I<THEMES>] [--fonts=I<FONTS>]"
msgstr ""

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "[--locales=I<LOCALES>] [--compress[=I<no>,I<xz>,I<gz>,I<lzo>]]"
msgstr ""

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "[-d | --directory=I<DIR>] [--grub-mkimage=I<FILE>]"
msgstr ""

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "I<SOURCE...>"
msgstr ""

#. type: SH
#: fedora-rawhide mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
#, no-wrap
msgid "DESCRIPTION"
msgstr ""

#. type: SH
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "OPTIONS"
msgstr ""

#. type: TP
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "--output=I<FILE>"
msgstr ""

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid ""
"Write the generated file to I<FILE>.  The default is to write to standard "
"output."
msgstr ""

#. type: TP
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "--format=I<FORMAT>"
msgstr ""

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "Generate an image in the specified I<FORMAT>.  Valid values are:"
msgstr ""

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid ""
"i386-coreboot, i386-multiboot, i386-pc, i386-pc-pxe, i386-efi, i386-"
"ieee1275, i386-qemu, x86_64-efi, mipsel-yeeloong-flash, mipsel-fuloong2f-"
"flash, mipself-loongson-elf, powerpc-ieee1275, sparc64-ieee1275-raw, sparc64-"
"ieee1275-cdcore, sparc64-ieee1275-aout, ia64-efi, mips-arc, mipsel-arc, "
"mipsel-qemu_mips-elf, mips-qemu_mips-flash, mipsel-qemu_mips-flash, mips-"
"qemu_mips-elf"
msgstr ""

#. type: TP
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "--compression=(I<xz>|I<none>|I<auto>)"
msgstr ""

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid ""
"Use one of I<xz>, I<none>, or I<auto> as the compression method for the core "
"image."
msgstr ""

#. type: TP
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "--modules=I<MODULES>"
msgstr ""

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "Pre-load modules specified by I<MODULES>."
msgstr ""

#. type: TP
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "--install-modules=I<MODULES>"
msgstr ""

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid ""
"Install only I<MODULES> and their dependencies.  The default is to install "
"all available modules."
msgstr ""

#. type: TP
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "--themes=I<THEMES>"
msgstr ""

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid ""
"Install I<THEMES>.  The default is to install the I<starfield> theme, if "
"available."
msgstr ""

#. type: TP
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "--fonts=I<FONTS>"
msgstr ""

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "Install I<FONTS>.  The default is to install the I<unicode> font."
msgstr ""

#. type: TP
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "--locales=I<LOCALES>"
msgstr ""

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid ""
"Install only locales listed in I<LOCALES>.  The default is to install all "
"available locales."
msgstr ""

#. type: TP
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "--compress[=I<no>,I<xz>,I<gz>,I<lzo>]"
msgstr ""

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "Compress GRUB files using the specified compression algorithm."
msgstr ""

#. type: TP
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "--directory=I<DIR>"
msgstr ""

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "Use images and modules in I<DIR>."
msgstr ""

#. type: TP
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "--grub-mkimage=I<FILE>"
msgstr ""

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid ""
"Use I<FILE> as B<grub-mkimage>.  The default is I</usr/bin/grub-mkimage>."
msgstr ""


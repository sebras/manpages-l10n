#: opensuse-leap-15-4
#, no-wrap
msgid ""
"void\n"
"consumeCpu2(int lim)\n"
"{\n"
"    int j;\n"
msgstr ""

#. type: Plain text
#: opensuse-leap-15-4
#, no-wrap
msgid ""
"void\n"
"x2(void)\n"
"{\n"
"    int j;\n"
msgstr ""


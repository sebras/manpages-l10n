msgid "NAME"
msgstr ""

#. type: Plain text
#: fedora-36
msgid ""
"B<grub-mkfont> \\(em Convert common font file formats into the PF2 format."
msgstr ""

#. type: SH
#: fedora-36 fedora-rawhide mageia-cauldron opensuse-leap-15-4
#: opensuse-tumbleweed
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
#: fedora-36
msgid "B<grub-mkfont> [--ascii-bitmaps] [-a | --force-autohint]"
msgstr ""

#. type: Plain text
#: fedora-36
msgid "[-b | --bold] [-c | --asce=I<NUM>] [-d | --desc=I<NUM>]"
msgstr ""

#. type: Plain text
#: fedora-36
msgid "[-i | --index=I<NUM>] [-n | --name=I<NAME>] [--no-bitmap]"
msgstr ""

#. type: Plain text
#: fedora-36
msgid "[--no-hinting] E<lt>-o | --output=I<FILE>E<gt>"
msgstr ""

#. type: Plain text
#: fedora-36
msgid "[-r | --range=I<FROM-TO>[I<,FROM-TO>]] [-s | --size=I<SIZE>]"
msgstr ""

#. type: Plain text
#: fedora-36
msgid "[-v | --verbose] [--width-spec] I<FONT_FILES>"
msgstr ""

#. type: SH
#: fedora-36 fedora-rawhide mageia-cauldron opensuse-leap-15-4
#: opensuse-tumbleweed
#, no-wrap
msgid "DESCRIPTION"
msgstr ""

#. type: Plain text
#: fedora-36
msgid ""
"B<grub-mkfont> converts font files from common formats into the PF2 format "
"used by GRUB."
msgstr ""


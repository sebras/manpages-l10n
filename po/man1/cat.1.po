#. type: TH
#, no-wrap
msgid "CAT"
msgstr "CAT"

#. type: TH
#, no-wrap
msgid "December 2016"
msgstr "Dezember 2016"

#. type: TH
#, no-wrap
msgid "GNU coreutils 8.26"
msgstr "GNU coreutils 8.26"

#. type: TH
#, no-wrap
msgid "User Commands"
msgstr "Dienstprogramme für Benutzer"

#. type: SH
#, no-wrap
msgid "NAME"
msgstr "BEZEICHNUNG"

#. type: Plain text
msgid "cat - concatenate files and print on the standard output"
msgstr "cat - Dateien aneinanderhängen und in die Standardausgabe schreiben"

#. type: SH
#, no-wrap
msgid "SYNOPSIS"
msgstr "ÜBERSICHT"

#. type: Plain text
msgid "B<cat> [I<\\,OPTION\\/>]... [I<\\,FILE\\/>]..."
msgstr "B<cat> [I<\\,OPTION\\/>]… [I<\\,DATEI\\/>]…"

#. type: SH
#, no-wrap
msgid "DESCRIPTION"
msgstr "BESCHREIBUNG"

#. type: Plain text
msgid "Concatenate FILE(s) to standard output."
msgstr "DATEI(en) auf Standardausgabe verketten."

#. type: Plain text
msgid "With no FILE, or when FILE is -, read standard input."
msgstr "Ohne DATEI oder wenn DATEI »-« ist, Standardeingabe lesen."

#. type: TP
#, no-wrap
msgid "B<-A>, B<--show-all>"
msgstr "B<-A>, B<--show-all>"

#. type: Plain text
msgid "equivalent to B<-vET>"
msgstr "äquivalent zu B<-vET>"

#. type: TP
#, no-wrap
msgid "B<-b>, B<--number-nonblank>"
msgstr "B<-b>, B<--number-nonblank>"

#. type: Plain text
msgid "number nonempty output lines, overrides B<-n>"
msgstr "nichtleere Ausgabezeilen nummerieren, überschreibt B<-n>"

#. type: TP
#, no-wrap
msgid "B<-e>"
msgstr "B<-e>"

#. type: Plain text
msgid "equivalent to B<-vE>"
msgstr "äquivalent zu B<-vE>"

#. type: TP
#, no-wrap
msgid "B<-E>, B<--show-ends>"
msgstr "B<-E>, B<--show-ends>"

#. type: Plain text
msgid "display $ at end of each line"
msgstr "$ am Ende jeder Zeile ausgeben"

#. type: TP
#, no-wrap
msgid "B<-n>, B<--number>"
msgstr "B<-n>, B<--number>"

#. type: Plain text
msgid "number all output lines"
msgstr "alle Ausgabezeilen nummerieren"

#. type: TP
#, no-wrap
msgid "B<-s>, B<--squeeze-blank>"
msgstr "B<-s>, B<--squeeze-blank>"

#. type: Plain text
msgid "suppress repeated empty output lines"
msgstr "aufeinander folgende Leerzeilen unterdrücken"

#. type: TP
#, no-wrap
msgid "B<-t>"
msgstr "B<-t>"

#. type: Plain text
msgid "equivalent to B<-vT>"
msgstr "äquivalent zu B<-vT>"

#. type: TP
#, no-wrap
msgid "B<-T>, B<--show-tabs>"
msgstr "B<-T>, B<--show-tabs>"

#. type: Plain text
msgid "display TAB characters as ^I"
msgstr "Tabulator‐Zeichen als ^I ausgeben"

#. type: TP
#, no-wrap
msgid "B<-u>"
msgstr "B<-u>"

#. type: Plain text
msgid "(ignored)"
msgstr "(ignoriert)"

#. type: TP
#, no-wrap
msgid "B<-v>, B<--show-nonprinting>"
msgstr "B<-v>, B<--show-nonprinting>"

#. type: Plain text
msgid "use ^ and M- notation, except for LFD and TAB"
msgstr "^‐ und M‐Notation benutzen, außer für LFD und Tabulatoren."

#. type: TP
#, no-wrap
msgid "B<--help>"
msgstr "B<--help>"

#. type: Plain text
msgid "display this help and exit"
msgstr "Diese Hilfe anzeigen und beenden"

#. type: TP
#, no-wrap
msgid "B<--version>"
msgstr "B<--version>"

#. type: Plain text
msgid "output version information and exit"
msgstr "Versionsinformation anzeigen und beenden"

#. type: SH
#, no-wrap
msgid "EXAMPLES"
msgstr "BEISPIELE"

#. type: TP
#, no-wrap
msgid "cat f - g"
msgstr "cat f - g"

#. type: Plain text
msgid "Output f's contents, then standard input, then g's contents."
msgstr ""
"Gibt den Inhalt von f aus, dann die Standardeingabe, schließlich den Inhalt "
"von g."

#. type: TP
#, no-wrap
msgid "cat"
msgstr "cat"

#. type: Plain text
msgid "Copy standard input to standard output."
msgstr "Kopiert die Standardeingabe in die Standardausgabe."

#. type: SH
#, no-wrap
msgid "AUTHOR"
msgstr "AUTOR"

#. type: Plain text
msgid "Written by Torbjorn Granlund and Richard M. Stallman."
msgstr "Geschrieben von Torbjörn Granlund und Richard M. Stallman."

#. type: SH
#, no-wrap
msgid "REPORTING BUGS"
msgstr "FEHLER BERICHTEN"

#. type: Plain text
msgid ""
"GNU coreutils online help: E<lt>http://www.gnu.org/software/coreutils/E<gt>"
msgstr ""
"Onlinehilfe für GNU coreutils: E<lt>http://www.gnu.org/software/coreutils/"
"E<gt>"

#. type: Plain text
msgid ""
"Report cat translation bugs to E<lt>http://translationproject.org/team/E<gt>"
msgstr ""
"Berichten Sie Fehler in der Übersetzung von cat an E<lt>http://"
"translationproject.org/team/de.htmlE<gt>"

#. type: SH
#, no-wrap
msgid "COPYRIGHT"
msgstr "COPYRIGHT"

#. type: Plain text
msgid ""
"Copyright \\(co 2016 Free Software Foundation, Inc.  License GPLv3+: GNU GPL "
"version 3 or later E<lt>http://gnu.org/licenses/gpl.htmlE<gt>."
msgstr ""
"Copyright \\(co 2016 Free Software Foundation, Inc. Lizenz GPLv3+: GNU GPL "
"Version 3 oder neuer E<lt>http://gnu.org/licenses/gpl.htmlE<gt>."

#. type: Plain text
msgid ""
"This is free software: you are free to change and redistribute it.  There is "
"NO WARRANTY, to the extent permitted by law."
msgstr ""
"Dies ist freie Software: Sie können sie verändern und weitergeben. Es gibt "
"KEINE GARANTIE, soweit gesetzlich zulässig."

#. type: SH
#, no-wrap
msgid "SEE ALSO"
msgstr "SIEHE AUCH"

#. type: Plain text
msgid "B<tac>(1)"
msgstr "B<tac>(1)"

#. type: Plain text
msgid ""
"Full documentation at: E<lt>http://www.gnu.org/software/coreutils/catE<gt>"
msgstr ""
"Vollständige Dokumentation unter: E<lt>http://www.gnu.org/software/coreutils/"
"catE<gt>"

#. type: Plain text
msgid "or available locally via: info \\(aq(coreutils) cat invocation\\(aq"
msgstr "oder lokal verfügbar mit: info \\(aq(coreutils) cat invocation\\(aq"

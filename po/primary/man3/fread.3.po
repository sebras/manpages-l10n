#. type: TH
#, no-wrap
msgid "FREAD"
msgstr "FREAD"

#. type: TH
#, no-wrap
msgid "2015-07-23"
msgstr "23. Juli 2015"

#. type: TH
#, no-wrap
msgid "GNU"
msgstr "GNU"

#. type: TH
#, no-wrap

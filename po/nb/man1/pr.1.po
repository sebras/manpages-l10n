#. type: TH
#: archlinux opensuse-tumbleweed
#, no-wrap
msgid "April 2022"
msgstr "April 2022"

#. type: TH
#: archlinux fedora-rawhide opensuse-tumbleweed
#, no-wrap
msgid "GNU coreutils 9.1"
msgstr "GNU coreutils 9.1"

#. type: TH
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
#, no-wrap
msgid "User Commands"
msgstr "Brukerkommandoer"

#. type: SH
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
#, no-wrap
msgid "NAME"
msgstr "NAVN"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
msgid "pr - convert text files for printing"
msgstr ""

#. type: SH
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
#, no-wrap
msgid "SYNOPSIS"
msgstr "OVERSIKT"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
msgid "B<pr> [I<\\,OPTION\\/>]... [I<\\,FILE\\/>]..."
msgstr "B<pr> [I<\\,VALG\\/>]... [I<\\,FIL\\/>]..."

#. type: SH
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
#, no-wrap
msgid "DESCRIPTION"
msgstr "BESKRIVELSE"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
msgid "Paginate or columnate FILE(s) for printing."
msgstr "Merk valgte FIL(er) med sidetall, eller gi dem spalter."

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
msgid "With no FILE, or when FILE is -, read standard input."
msgstr ""
"Hvis ingen FIL er valgt, eller hvis FIL er «-», leser programmet standard "
"inndata."

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
msgid ""
"Mandatory arguments to long options are mandatory for short options too."
msgstr ""
"Argumenter som er obligatoriske for lange valg, er også obligatoriske for "
"korte valg."

#. type: TP
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
#, no-wrap
msgid "+FIRST_PAGE[:LAST_PAGE], B<--pages>=I<\\,FIRST_PAGE[\\/>:LAST_PAGE]"
msgstr "+FØRSTE_SIDE[:SISTE_SIDE], B<--pages>=I<\\,FØRSTE_SIDE[\\/>:SISTE_SIDE]"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
msgid "begin [stop] printing with page FIRST_[LAST_]PAGE"
msgstr "Start [stans] utskrift ved FØRSTE_[SISTE_]SIDE."

#. type: TP
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
#, no-wrap
msgid "B<-COLUMN>, B<--columns>=I<\\,COLUMN\\/>"
msgstr "B<-KOL>, B<--columns>=I<\\,KOL\\/>"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
msgid ""
"output COLUMN columns and print columns down, unless B<-a> is used. Balance "
"number of lines in the columns on each page"
msgstr ""
"kriv ut KOL antall kolonner og skriv kolonner nedover, med mindre B<-a> er "
"valgt. Balanser antall linjer i kolonnene på neste side."

#. type: TP
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
#, no-wrap
msgid "B<-a>, B<--across>"
msgstr "B<-a>, B<--across>"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
msgid "print columns across rather than down, used together with B<-COLUMN>"
msgstr ""
"Skriv kolonner på tvers i stedet for nedover (brukes sammen med B<-KOLONNE>)."

#. type: TP
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
#, no-wrap
msgid "B<-c>, B<--show-control-chars>"
msgstr "B<-c>, B<--show-control-chars>"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
msgid "use hat notation (^G) and octal backslash notation"
msgstr "Bruk hattnotasjon (^G) og oktal backslah-notasjon."

#. type: TP
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
#, no-wrap
msgid "B<-d>, B<--double-space>"
msgstr "B<-d>, B<--double-space>"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
msgid "double space the output"
msgstr "Bruk dobbel linjeavstand."

#. type: TP
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
#, no-wrap
msgid "B<-D>, B<--date-format>=I<\\,FORMAT\\/>"
msgstr "B<-D>, B<--date-format>=I<\\,FORMAT\\/>"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
msgid "use FORMAT for the header date"
msgstr "Bruk valgt datoFORMAT i topptekst."

#. type: TP
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
#, no-wrap
msgid "B<-e[CHAR[WIDTH]]>, B<--expand-tabs>[=I<\\,CHAR[WIDTH]\\/>]"
msgstr "B<-e[TEGN[BREDDE]]>, B<--expand-tabs>[=I<\\,TEGN[BREDDE]\\/>]"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
msgid "expand input CHARs (TABs) to tab WIDTH (8)"
msgstr "Utvid inndataTEGN (TAB) til tabulatorBREDDE (8)."

#. type: TP
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
#, no-wrap
msgid "B<-F>, B<-f>, B<--form-feed>"
msgstr "B<-F>, B<-f>, B<--form-feed>"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
msgid ""
"use form feeds instead of newlines to separate pages (by a 3-line page "
"header with B<-F> or a 5-line header and trailer without B<-F>)"
msgstr ""
"Bruk sideskift i stedet for linjeskift for å separere sider. (ved en 3-"
"linjers topptekst med B<-F> eller en 5-linjers topptekst og bunntekst uten "
"B<-F>)."

#. type: TP
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
#, no-wrap
msgid "B<-h>, B<--header>=I<\\,HEADER\\/>"
msgstr "B<-h>, B<--header>=I<\\,OVERSKRIFT\\/>"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
msgid ""
"use a centered HEADER instead of filename in page header, B<-h> \"\" prints "
"a blank line, don't use B<-h>\"\""
msgstr ""
"Bruk en midtstilt overskrift, i stedet for filnavnet i sideoverskriften. «B<-"
"h> \"\"» skriver ut en tom linje (ikke bruk «B<-h>\"\"»)."

#. type: TP
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
#, no-wrap
msgid "B<-i[CHAR[WIDTH]]>, B<--output-tabs>[=I<\\,CHAR[WIDTH]\\/>]"
msgstr "B<-i[TEGN[BREDDE]]>, B<--output-tabs>[=I<\\,TEGN[BREDDE]\\/>]"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
msgid "replace spaces with CHARs (TABs) to tab WIDTH (8)"
msgstr "Erstatt mellomrom med TEGN (TAB-er) til tabulatorBREDDE (8)."

#. type: TP
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
#, no-wrap
msgid "B<-J>, B<--join-lines>"
msgstr "B<-J>, B<--join-lines>"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
msgid ""
"merge full lines, turns off B<-W> line truncation, no column alignment, B<--"
"sep-string>[=I<\\,STRING\\/>] sets separators"
msgstr ""
"Slå sammen hele linjer (skrur av B<-W>-valget linjeforkortelse uten "
"kolonneoppstilling), B<--sep-string>[=I<\\,STRENG\\/>] velger skilletegn."

#. type: TP
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
#, no-wrap
msgid "B<-l>, B<--length>=I<\\,PAGE_LENGTH\\/>"
msgstr "B<-l>, B<--length>=I<\\,SIDELENGDE\\/>"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
#, fuzzy
#| msgid ""
#| "  -l, --length=PAGE_LENGTH\n"
#| "                    set the page length to PAGE_LENGTH (66) lines\n"
#| "                    (default number of lines of text 56, and with -F "
#| "63).\n"
#| "                    implies -t if PAGE_LENGTH <= 10\n"
msgid ""
"set the page length to PAGE_LENGTH (66) lines (default number of lines of "
"text 56, and with B<-F> 63).  implies B<-t> if PAGE_LENGTH E<lt>= 10"
msgstr ""
"Velg SIDELENGDE i form av antall (66) linjer (standard antall tekstfylte "
"linjer er 56, eller 63 ved bruk av B<-F>)."

#. type: TP
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
#, no-wrap
msgid "B<-m>, B<--merge>"
msgstr "B<-m>, B<--merge>"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
msgid ""
"print all files in parallel, one in each column, truncate lines, but join "
"lines of full length with B<-J>"
msgstr ""
"Skriv ut alle filer parallelt, med én fil per kolonne. Bruk valget B<-J> for "
"å korte ned linjer hvis nødvendig, og slå sammen fullstendige linjer."

#. type: TP
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
#, no-wrap
msgid "B<-n[SEP[DIGITS]]>, B<--number-lines>[=I<\\,SEP[DIGITS]\\/>]"
msgstr "B<-n[SEP[SIFF]]>, B<--number-lines>[=I<\\,SEP[SIFF]\\/>]"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
msgid ""
"number lines, use DIGITS (5) digits, then SEP (TAB), default counting starts "
"with 1st line of input file"
msgstr ""
"Nummerer linjer, med angitt antall SIFFre (5), og deretter SEP (TAB). "
"Telling starter ved første linje i inndatafila som standard."

#. type: TP
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
#, no-wrap
msgid "B<-N>, B<--first-line-number>=I<\\,NUMBER\\/>"
msgstr "B<-N>, B<--first-line-number>=I<\\,NUMMER\\/>"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
msgid ""
"start counting with NUMBER at 1st line of first page printed (see "
"+FIRST_PAGE)"
msgstr ""
"Start tellinga fra valgt linjeNUMMER etter første linje på første "
"utskriftsside (se +FØRSTE_SIDE)."

#. type: TP
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
#, no-wrap
msgid "B<-o>, B<--indent>=I<\\,MARGIN\\/>"
msgstr "B<-o>, B<--indent>=I<\\,MARG\\/>"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
msgid ""
"offset each line with MARGIN (zero) spaces, do not affect B<-w> or B<-W>, "
"MARGIN will be added to PAGE_WIDTH"
msgstr ""
"Forskyv hver linje med valgt MARG (tomrom). Dette påvirker hverken B<-w> "
"eller B<-W>. MARG legges til i BREDDE."

#. type: TP
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
#, no-wrap
msgid "B<-r>, B<--no-file-warnings>"
msgstr "B<-r>, B<--no-file-warnings>"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
msgid "omit warning when a file cannot be opened"
msgstr "Ikke gi advarsel når programmet ikke klarer å åpne en fil."

#. type: TP
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
#, no-wrap
msgid "B<-s[CHAR]>, B<--separator>[=I<\\,CHAR\\/>]"
msgstr "B<-s[TEGN]>, B<--separator>[=I<\\,TEGN\\/>]"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
msgid ""
"separate columns by a single character, default for CHAR is the "
"E<lt>TABE<gt> character without B<-w> and 'no char' with B<-w>.  B<-s[CHAR]> "
"turns off line truncation of all 3 column options (B<-COLUMN>|-a B<-COLUMN>|-"
"m) except B<-w> is set"
msgstr ""
"Skill kolonner fra hverandre med ett enkelt tegn (TEGN er E<lt>TABE<gt> som "
"standard, uten B<-w> og «no char» med B<-w>). Skru av linjeforkortelse for "
"alle 3 kolonnevalg med B<-s[TEGN]> (B<-KOL>|-a B<-KOL>|-m), unntatt når B<-"
"w> er valgt."

#. type: TP
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
#, no-wrap
msgid "B<-S[STRING]>, B<--sep-string>[=I<\\,STRING\\/>]"
msgstr "B<-S[STRENG]>, B<--sep-string>[=I<\\,STRENG\\/>]"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
msgid ""
"separate columns by STRING, without B<-S>: Default separator E<lt>TABE<gt> "
"with B<-J> and E<lt>spaceE<gt> otherwise (same as B<-S>\" \"), no effect on "
"column options"
msgstr ""
"Skill kolonner fra hverandre med STRENG, uten B<-S>. Standard skilletegn er "
"E<lt>TABE<gt> ved bruk av B<-J>, og ellers E<lt>mellomromE<gt> (tilsvarer B<-"
"S>\" \"). Dette påvirker ikke øvrige kolonnevalg."

#. type: TP
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
#, no-wrap
msgid "B<-t>, B<--omit-header>"
msgstr "B<-t>, B<--omit-header>"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
msgid "omit page headers and trailers; implied if PAGE_LENGTH E<lt>= 10"
msgstr ""
"Utelat start- og slutttegn for sider. (gjelder implisitt hvis «PAGE_LENGTH» "
"er mindre enn 10)."

#. type: TP
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
#, no-wrap
msgid "B<-T>, B<--omit-pagination>"
msgstr "B<-T>, B<--omit-pagination>"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
msgid ""
"omit page headers and trailers, eliminate any pagination by form feeds set "
"in input files"
msgstr ""
"Utelat topp- og bunntekst, og fjern eventuell sidetallsmerking fra utforming "
"i inndatafiler."

#. type: TP
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
#, no-wrap
msgid "B<-v>, B<--show-nonprinting>"
msgstr "B<-v>, B<--show-nonprinting>"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
msgid "use octal backslash notation"
msgstr "Bruk oktal skråstreknotasjon."

#. type: TP
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
#, no-wrap
msgid "B<-w>, B<--width>=I<\\,PAGE_WIDTH\\/>"
msgstr "B<-w>, B<--width>=I<\\,BREDDE\\/>"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
msgid ""
"set page width to PAGE_WIDTH (72) characters for multiple text-column output "
"only, B<-s[char]> turns off (72)"
msgstr ""
"Velg sidebredde i form av antall tegn, i henhold til BREDDE (72), kun for "
"utskrift av flere tekstkolonner. B<-s[tegn]> skrur av (72)."

#. type: TP
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
#, no-wrap
msgid "B<-W>, B<--page-width>=I<\\,PAGE_WIDTH\\/>"
msgstr "B<-W>, B<--page-width>=I<\\,BREDDE\\/>"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
msgid ""
"set page width to PAGE_WIDTH (72) characters always, truncate lines, except "
"B<-J> option is set, no interference with B<-S> or B<-s>"
msgstr ""
"Velg sidebredde i form av antall tegn, i henhold til BREDDE (72), og kort "
"ned linjer med mindre B<-J> er valgt. Dette påvirker ikke valgene B<-S> "
"eller B<-s>."

#. type: TP
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
#, no-wrap
msgid "B<--help>"
msgstr "B<--help>"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
msgid "display this help and exit"
msgstr "Vis denne hjelpeteksten og avslutt."

#. type: TP
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
#, no-wrap
msgid "B<--version>"
msgstr "B<--version>"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
msgid "output version information and exit"
msgstr "Vis versjonsinformasjon og avslutt."

#. type: SH
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
#, no-wrap
msgid "AUTHOR"
msgstr "OPPHAVSMANN"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
msgid "Written by Pete TerMaat and Roland Huebner."
msgstr "Skrevet av Pete TerMaat og Roland Huebner."

#. type: SH
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
#, no-wrap
msgid "REPORTING BUGS"
msgstr "RAPPORTERING AV FEIL"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
msgid ""
"GNU coreutils online help: E<lt>https://www.gnu.org/software/coreutils/E<gt>"
msgstr ""
"Få hjelp til bruk av GNU coreutils på nett: E<lt>https://www.gnu.org/"
"software/coreutils/E<gt>"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
msgid ""
"Report any translation bugs to E<lt>https://translationproject.org/team/E<gt>"
msgstr ""
"Rapporter oversettelsesfeil til E<lt>https://translationproject.org/team/nb."
"htmlE<gt>"

#. type: SH
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
#, no-wrap
msgid "COPYRIGHT"
msgstr "OPPHAVSRETT"

#. type: Plain text
#: archlinux fedora-rawhide opensuse-tumbleweed
msgid ""
"Copyright \\(co 2022 Free Software Foundation, Inc.  License GPLv3+: GNU GPL "
"version 3 or later E<lt>https://gnu.org/licenses/gpl.htmlE<gt>."
msgstr ""
"Copyright \\(co 2022 Free Software Foundation, Inc.  Lisens GPLv3+: GNU GPL "
"versjon 3 eller senere E<lt>https://gnu.org/licenses/gpl.htmlE<gt>."


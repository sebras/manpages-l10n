#, no-wrap
msgid "GRUB-MKNETDIR"
msgstr "GRUB-MKNETDIR"

#. type: TH
#: archlinux
#, fuzzy, no-wrap
#| msgid "August 2021"
msgid "August 2022"
msgstr "august 2021"

#. type: TH
#: archlinux
#, fuzzy, no-wrap
#| msgid "GRUB 2:2.06.r261.g2f4430cc0-1"
msgid "GRUB 2:2.06.r297.g0c6c1aff2-1"

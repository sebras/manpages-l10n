#. type: TH
#: opensuse-leap-15-4
#, no-wrap
msgid "PWDX"
msgstr "PWDX"

#. type: TH
#: opensuse-leap-15-4
#, no-wrap
msgid "June 2011"
msgstr "juni 2011"

#. type: TH
#: opensuse-leap-15-4
#, no-wrap
msgid "procps-ng"
msgstr "procps-ng"

#. type: TH
#: opensuse-leap-15-4
#, no-wrap
msgid "User Commands"
msgstr "Brugerkommandoer"

#. type: SH
#: opensuse-leap-15-4
#, no-wrap
msgid "NAME"
msgstr "NAVN"

#. type: Plain text
#: opensuse-leap-15-4
msgid "pwdx - report current working directory of a process"
msgstr "pwdx - rapporter nuværende arbejdsmappe for en proces"

#. type: SH
#: opensuse-leap-15-4
#, no-wrap
msgid "SYNOPSIS"
msgstr "SYNOPSIS"

#. type: Plain text
#: opensuse-leap-15-4
msgid "B<pwdx> [I<options>] I<pid> [...]"
msgstr "B<pwdx> [I<tilvalg>] I<pid> [...]"

#. type: SH
#: opensuse-leap-15-4
#, no-wrap
msgid "OPTIONS"
msgstr "TILVALG"

#. type: TP
#: opensuse-leap-15-4
#, no-wrap
msgid "B<-V>, B<--version>"
msgstr "B<-V>, B<--version>"

#. type: Plain text
#: opensuse-leap-15-4
msgid "Output version information and exit."
msgstr "Vis versionsinformation og afslut."

#. type: TP
#: opensuse-leap-15-4
#, no-wrap
msgid "B<-h>, B<--help>"
msgstr "B<-h>, B<--help>"

#. type: Plain text
#: opensuse-leap-15-4
msgid "Output help screen and exit."
msgstr "Vis hjælpeskærm og afslut."

#. type: SH
#: opensuse-leap-15-4
#, no-wrap
msgid "SEE ALSO"
msgstr "SE OGSÅ"

#. type: Plain text
#: opensuse-leap-15-4
msgid "B<ps>(1), B<pgrep>(1)"
msgstr "B<ps>(1), B<pgrep>(1)"

#. type: SH
#: opensuse-leap-15-4
#, no-wrap
msgid "STANDARDS"
msgstr "STANDARDER"

#. type: Plain text
#: opensuse-leap-15-4
msgid "No standards apply, but pwdx looks an awful lot like a SunOS command."
msgstr "Ingen gældende standarder, men pwdx ligner meget en SunOS-kommando."

#. type: SH
#: opensuse-leap-15-4
#, no-wrap
msgid "AUTHOR"
msgstr "FORFATTER"

#. type: Plain text
#: opensuse-leap-15-4
msgid "E<.UR nmiell@gmail.com> Nicholas Miell E<.UE> wrote pwdx in 2004."
msgstr "E<.UR nmiell@gmail.com> Nicholas Miell E<.UE> skrev pwdx in 2004."

#. type: SH
#: opensuse-leap-15-4
#, no-wrap
msgid "REPORTING BUGS"
msgstr "FEJLRAPPORTER"

#. type: Plain text
#: opensuse-leap-15-4
msgid "Please send bug reports to E<.UR procps@freelists.org> E<.UE>"
msgstr "Send venligst fejlrapporter til E<.UR procps@freelists.org> E<.UE>"

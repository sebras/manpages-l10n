#. type: TH
#: opensuse-leap-15-4
#, no-wrap
msgid "PRTSTAT"
msgstr "PRTSTAT"

#. type: TH
#: opensuse-leap-15-4
#, no-wrap
msgid "2016-06-18"
msgstr "18. juni 2016"

#. type: TH
#: opensuse-leap-15-4
#, no-wrap
msgid "psmisc"
msgstr "psmisc"

#. type: TH
#: opensuse-leap-15-4
#, no-wrap
msgid "User Commands"
msgstr "Brugerkommandoer"

#. type: SH
#: opensuse-leap-15-4
#, no-wrap
msgid "NAME"
msgstr "NAVN"

#. type: Plain text
#: opensuse-leap-15-4
msgid "prtstat - print statistics of a process"
msgstr "prtstat - vis statistikken for en proces"

#. type: SH
#: opensuse-leap-15-4
#, no-wrap
msgid "SYNOPSIS"
msgstr "SYNOPSIS"

#. type: Plain text
#: opensuse-leap-15-4
msgid "B<prtstat> [B<-r>|B<--raw>] I<pid>"
msgstr "B<prtstat> [B<-r>|B<--raw>] I<pid>"

#. type: Plain text
#: opensuse-leap-15-4
msgid "B<prtstat> -VB<|>--version"
msgstr "B<prtstat> -VB<|>--version"

#. type: SH
#: opensuse-leap-15-4
#, no-wrap
msgid "DESCRIPTION"
msgstr "BESKRIVELSE"

#. type: Plain text
#: opensuse-leap-15-4
msgid ""
"B<prtstat> prints the statistics of the specified process.  This information "
"comes from the I</proc/PID/stat> file."
msgstr ""
"B<prtstat> viser statistikken for den specificerede proces. Denne "
"information kommer fra filen I</proc/PID/stat>."

#. type: SH
#: opensuse-leap-15-4
#, no-wrap
msgid "OPTIONS"
msgstr "TILVALG"

#. type: TP
#: opensuse-leap-15-4
#, no-wrap
msgid "B<-r>,B<\\ --raw>"
msgstr "B<-r>,B<\\ --raw>"

#. type: Plain text
#: opensuse-leap-15-4
msgid "Print the information in raw format."
msgstr "Vis informationen i formatet raw (rå)."

#. type: TP
#: opensuse-leap-15-4
#, no-wrap
msgid "B<-V>,B<\\ --version>"
msgstr "B<-V>,B<\\ --version>"

#. type: Plain text
#: opensuse-leap-15-4
msgid "Show the version information for B<prtstat>."
msgstr "Vis versionsinformationen for B<prtstat>."

#. type: SH
#: opensuse-leap-15-4
#, no-wrap
msgid "FILES"
msgstr "FILER"

#. type: TP
#: opensuse-leap-15-4
#, no-wrap
msgid "/proc/E<lt>PIDE<gt>/stat"
msgstr "/proc/E<lt>PIDE<gt>/stat"

#. type: Plain text
#: opensuse-leap-15-4
msgid "source of the information B<prtstat> uses"
msgstr "kilden for informationen brugt af B<prtstat>"

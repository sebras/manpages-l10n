msgstr "B<\"djgpp\">, B<\"mingw\">"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-rawhide mageia-cauldron
#: opensuse-tumbleweed
msgid "Microsoft Windows, using either DJGPP, or MinGW, respectively."
msgstr "Microsoft Windows, compilato tramite DJGPP o MinGW, rispettivamente."

#. type: TP
#: archlinux debian-bullseye debian-unstable fedora-rawhide mageia-cauldron
#: opensuse-tumbleweed
#, no-wrap
msgid "B<\"os2\">"
msgstr "B<\"os2\">"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-rawhide mageia-cauldron
#: opensuse-tumbleweed
msgid "OS/2."
msgstr "OS/2."

#. type: TP
#: archlinux debian-bullseye debian-unstable fedora-rawhide mageia-cauldron
#: opensuse-tumbleweed
#, no-wrap

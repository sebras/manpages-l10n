msgid "GRUB-MKLAYOUT"
msgstr "GRUB-MKLAYOUT"

#. type: TH
#: fedora-36 fedora-rawhide mageia-cauldron
#, no-wrap
msgid "Wed Feb 26 2014"
msgstr "У среду 26.02.2014"

#. type: SH
#: fedora-36 fedora-rawhide mageia-cauldron opensuse-leap-15-4
#: opensuse-tumbleweed
#, no-wrap
msgid "NAME"
msgstr "НАЗИВ"

#. type: Plain text
#: fedora-36 fedora-rawhide mageia-cauldron
msgid "B<grub-mklayout> \\(em Generate a GRUB keyboard layout file."
msgstr "B<grub-mklayout> \\(em Прави датотеку распореда тастатуре ГРУБ-а."

#. type: SH
#: fedora-36 fedora-rawhide mageia-cauldron opensuse-leap-15-4
#: opensuse-tumbleweed
#, no-wrap
msgid "SYNOPSIS"
msgstr "УВОД"

#. type: Plain text
#: fedora-36 fedora-rawhide mageia-cauldron
msgid "B<grub-mklayout> [-i | --input=I<FILE>] [-o | --output=I<FILE>]"
msgstr ""
"B<grub-mklayout> [-i | --input=I<ДАТОТЕКА>] [-o | --output=I<ДАТОТЕКА>]"

#. type: Plain text
#: fedora-36 fedora-rawhide mageia-cauldron
msgid "[-v | --verbose]"
msgstr "[-v | --verbose]"

#. type: SH
#: fedora-36 fedora-rawhide mageia-cauldron opensuse-leap-15-4
#: opensuse-tumbleweed
#, no-wrap
msgid "DESCRIPTION"
msgstr "ОПИС"

#. type: Plain text
#: fedora-36 fedora-rawhide mageia-cauldron
msgid ""
"B<grub-mklayout> generates a GRUB keyboard layout description which "
"corresponds with the Linux console layout description given as input."
msgstr ""

#. type: SH
#: fedora-36 fedora-rawhide mageia-cauldron
#, no-wrap
msgid "OPTIONS"
msgstr "ОПЦИЈЕ"

#. type: TP
#: fedora-36 fedora-rawhide mageia-cauldron
#, no-wrap
msgid "--input=I<FILE>"
msgstr "--input=I<ДАТОТЕКА>"

#. type: Plain text
#: fedora-36 fedora-rawhide mageia-cauldron
msgid ""
"Use I<FILE> as the input.  The default value is the standard input device."
msgstr ""

#. type: TP
#: fedora-36 fedora-rawhide mageia-cauldron
#, no-wrap
msgid "--output=I<FILE>"
msgstr "--output=I<ДАТОТЕКА>"

#. type: Plain text
#: fedora-36 fedora-rawhide mageia-cauldron
msgid ""
"Use I<FILE> as the output.  The default value is the standard output device."
msgstr ""

#. type: TP
#: fedora-36 fedora-rawhide mageia-cauldron
#, no-wrap
msgid "--verbose"
msgstr "--verbose"

#. type: Plain text
#: fedora-36 fedora-rawhide mageia-cauldron
msgid "Print verbose messages."
msgstr "Исписује опширне поруке."

#. type: SH
#: fedora-36 fedora-rawhide mageia-cauldron opensuse-leap-15-4
#: opensuse-tumbleweed
#, no-wrap
msgid "SEE ALSO"
msgstr "ВИДИТЕ ТАКОЂЕ"


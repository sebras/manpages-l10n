#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
#, no-wrap
msgid "DESCRIPTION"
msgstr "ОПИС"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
msgid "Convert rec data into csv data."
msgstr "Претворите податке записа у цсв податке."

#. type: TP
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
#, no-wrap
msgid "B<-d>, B<--delim>=I<\\,char\\/>"
msgstr "B<-d>, B<--delim>=I<\\,знак\\/>"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
msgid "sets the deliminator (default ',')"
msgstr "подешава раздвајач (основно је „,“)"

#. type: TP
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
#, no-wrap
msgid "B<-t>, B<--type>=I<\\,TYPE\\/>"
msgstr "B<-t>, B<--type>=I<\\,ВРСТА\\/>"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
msgid ""
"record set to convert to csv; if this parameter is omitted then the default "
"record set is used"
msgstr ""
"скуп записа за претварање у цсв; ако је овај параматар изостављен онда се "
"користи основни скуп записа"

#. type: TP
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
#, no-wrap
msgid "B<-S>, B<--sort>=I<\\,FIELDS\\/>"
msgstr "B<-S>, B<--sort>=I<\\,ПОЉА\\/>"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
msgid "sort the output by the specified fields."
msgstr "ређа резултат према наведеном пољу."

#. type: TP
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
#, no-wrap
msgid "B<--help>"
msgstr "B<--help>"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
msgid "print a help message and exit."

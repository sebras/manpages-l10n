"    ch = s & 0xff;\n"
"    if (s & mask)\n"
"        ch |= 0x100;\n"
"    attrib = ((s & \\(timask) E<gt>E<gt> 8);\n"
"    printf(\"ch=%#03x attrib=%#02x\\en\", ch, attrib);\n"
"    s \\(ha= 0x1000;\n"
"    (void) lseek(fd, -2, SEEK_CUR);\n"
"    (void) write(fd, &s, 2);\n"
"    exit(EXIT_SUCCESS);\n"
"}\n"


#: opensuse-leap-15-4
#, no-wrap
msgid "P"
msgstr "P"

#. type: IP
#: opensuse-leap-15-4
#, no-wrap
msgid "P'"
msgstr "P'"

#. type: IP
#: opensuse-leap-15-4
#, no-wrap
msgid "F"
msgstr "F"

#. type: IP
#: opensuse-leap-15-4
#, no-wrap
msgid "cap_bset"

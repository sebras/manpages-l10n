#. type: Plain text
#. #-#-#-#-#  debian-bullseye: fsck.fat.8.pot (PACKAGE VERSION)  #-#-#-#-#
#. type: Plain text
#. #-#-#-#-#  debian-unstable: fsck.fat.8.pot (PACKAGE VERSION)  #-#-#-#-#

#. type: Plain text
#: archlinux debian-bullseye debian-unstable mageia-cauldron opensuse-leap-15-3
#: opensuse-tumbleweed
msgid "systemd-rfkill\\&.service"
msgstr "systemd-rfkill\\&.service"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable mageia-cauldron opensuse-leap-15-3
#: opensuse-tumbleweed
msgid "systemd-rfkill\\&.socket"
msgstr "systemd-rfkill\\&.socket"


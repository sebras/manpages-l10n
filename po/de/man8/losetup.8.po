#. type: Plain text
#: debian-bullseye
msgid "B<losetup -l> [B<-a>]"
msgstr "B<losetup -l> [B<-a>]"

#. type: Plain text
#: debian-bullseye
msgid "B<losetup -j> I<file> [B<-o> I<offset>]"
msgstr "B<losetup -j> I<Datei> [B<-o> I<Versatz>]"

#. type: Plain text

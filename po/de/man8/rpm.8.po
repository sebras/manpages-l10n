#: debian-bullseye debian-unstable opensuse-leap-15-4
#, no-wrap
msgid "rpmrc Configuration"
msgstr "Rpmrc-Konfiguration"

#. type: Plain text
#: debian-bullseye
#, no-wrap
msgid ""
"I</usr/lib/rpm/rpmrc>\n"
"I</usr/lib/rpm/E<lt>vendorE<gt>/rpmrc>\n"
"I</etc/rpmrc>\n"

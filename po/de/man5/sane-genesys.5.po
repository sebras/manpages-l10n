#. type: TP
#. #-#-#-#-#  debian-buster: sane-genesys.5.pot (PACKAGE VERSION)  #-#-#-#-#
#. type: Plain text
#. #-#-#-#-#  debian-unstable: sane-genesys.5.pot (PACKAGE VERSION)  #-#-#-#-#
#. type: Plain text
#. #-#-#-#-#  fedora-rawhide: sane-genesys.5.pot (PACKAGE VERSION)  #-#-#-#-#

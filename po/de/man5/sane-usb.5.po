#: archlinux debian-bullseye debian-unstable fedora-rawhide mageia-cauldron
#: opensuse-tumbleweed
msgid ""
"B<sane>(7), B<sane-find-scanner>(1), B<sane-\"backendname\">(5), B<sane-"
"scsi>(5)"
msgstr ""
"B<sane>(7), B<sane-find-scanner>(1), B<sane-\"backendname\">(5), B<sane-"
"scsi>(5)"


#, no-wrap
msgid ""
"    for line_number, line in enumerate(f):\n"
"        line = line\\&.rstrip()\n"
"        if not line or line\\&.startswith(\\*(Aq#\\*(Aq):\n"
"            continue\n"
"        if m := re\\&.match(r\\*(Aq([A-Z][A-Z_0-9]+)=(\\&.*)\\*(Aq, line):\n"
"            name, val = m\\&.groups()\n"
"            if val and val[0] in \\*(Aq\"\\e\\*(Aq\\*(Aq:\n"
"                val = ast\\&.literal_eval(val)\n"
"            yield name, val\n"
"        else:\n"
"            print(f\\*(Aq{filename}:{line_number + 1}: bad line {line!r}\\*(Aq,\n"
"                  file=sys\\&.stderr)\n"
msgstr ""
"    for line_number, line in enumerate(f):\n"
"        line = line\\&.rstrip()\n"
"        if not line or line\\&.startswith(\\*(Aq#\\*(Aq):\n"
"            continue\n"
"        if m := re\\&.match(r\\*(Aq([A-Z][A-Z_0-9]+)=(\\&.*)\\*(Aq, line):\n"
"            name, val = m\\&.groups()\n"
"            if val and val[0] in \\*(Aq\"\\e\\*(Aq\\*(Aq:\n"
"                val = ast\\&.literal_eval(val)\n"
"            yield name, val\n"
"        else:\n"
"            print(f\\*(Aq{filename}:{line_number + 1}: ungültige Zeile {line!r}\\*(Aq,\n"
"                  file=sys\\&.stderr)\n"

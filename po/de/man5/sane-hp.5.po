#, no-wrap
msgid ""
"\\f(CR/dev/scanner\n"
"# this is a comment\n"
"/dev/hpscan\n"
"option connect-device\\fR\n"
msgstr ""
"\\f(CR/dev/scanner\n"
"# dies ist ein Kommentar\n"

#: archlinux
msgid "sane(7), saned(8), sane-dll(5), scanimage(1)"
msgstr "sane(7), saned(8), sane-dll(5), scanimage(1)"

#. type: Plain text
#: archlinux
msgid "I<http://www.penguin-breeder.org/?page=sane-net>"
msgstr "I<http://www.penguin-breeder.org/?page=sane-net>"


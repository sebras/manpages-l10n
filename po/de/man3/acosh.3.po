#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-37 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-5 opensuse-tumbleweed
msgid "acosh, acoshf, acoshl - inverse hyperbolic cosine function"
msgstr "acosh, acoshf, acoshl - Areakosinus-Hyperbolicus-Funktion"

#. type: SH
#: archlinux debian-bullseye debian-unstable fedora-37 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-5 opensuse-tumbleweed
#, no-wrap
msgid "SYNOPSIS"
msgstr "ÜBERSICHT"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-37 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-5 opensuse-tumbleweed
#, no-wrap
msgid "B<#include E<lt>math.hE<gt>>\n"
msgstr "B<#include E<lt>math.hE<gt>>\n"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-37 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-5 opensuse-tumbleweed
#, no-wrap
msgid ""
"B<double acosh(double >I<x>B<);>\n"
"B<float acoshf(float >I<x>B<);>\n"
"B<long double acoshl(long double >I<x>B<);>\n"
msgstr ""
"B<double acosh(double >I<x>B<);>\n"
"B<float acoshf(float >I<x>B<);>\n"
"B<long double acoshl(long double >I<x>B<);>\n"


#.  pow(|x|<1, -INFINITY) = INFINITY
#. type: Plain text
#. #-#-#-#-#  debian-bullseye: pow.3.pot (PACKAGE VERSION)  #-#-#-#-#
#. type: Plain text
#. #-#-#-#-#  debian-unstable: pow.3.pot (PACKAGE VERSION)  #-#-#-#-#

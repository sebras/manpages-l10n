"echo -e \\(aq1 0 A\\(rsn2 1 AA\\(rsn3 1 AB\\(rsn4 2 AAA\\(rsn5 2 AAB\\(aq | column --tree-id 1 --tree-parent 2 --tree 3\n"
"1  0  A\n"
"2  1  |-AA\n"
"4  2  | |-AAA\n"
"5  2  | `-AAB\n"
"3  1  `-AB\n"
msgstr ""
"echo -e \\(aq1 0 A\\(rsn2 1 AA\\(rsn3 1 AB\\(rsn4 2 AAA\\(rsn5 2 AAB\\(aq | column --tree-id 1 --tree-parent 2 --tree 3\n"

#. type: TH
#: debian-buster debian-unstable fedora-rawhide
#, no-wrap
msgid "RECINF"
msgstr "RECINF"

#. type: TH
#: debian-buster
#, no-wrap
msgid "March 2014"
msgstr "März 2014"

#. type: TH
#: debian-buster
#, no-wrap
msgid "recinf 1.7"
msgstr "recinf 1.7"

#. type: TH
#: debian-buster debian-unstable fedora-rawhide
#, no-wrap
msgid "User Commands"
msgstr "Dienstprogramme für Benutzer"

#. type: SH
#: debian-buster debian-unstable fedora-rawhide
#, no-wrap
msgid "NAME"
msgstr "BEZEICHNUNG"

#. type: Plain text
#: debian-buster debian-unstable fedora-rawhide
msgid "recinf - print information about a recfile"
msgstr "recinf - Informationen zu einer REC-Datei ausgeben"

#. type: SH
#: debian-buster debian-unstable fedora-rawhide
#, no-wrap
msgid "SYNOPSIS"
msgstr "ÜBERSICHT"

#. type: Plain text
#: debian-buster
msgid "B<recinf> [I<OPTION>]... [I<FILE>]..."
msgstr "B<recinf> [I<OPTION>]… [I<DATEI>]…"

#. type: SH
#: debian-buster debian-unstable fedora-rawhide
#, no-wrap
msgid "DESCRIPTION"
msgstr "BESCHREIBUNG"

#. type: Plain text
#: debian-buster debian-unstable fedora-rawhide
msgid "Print information about the types of records stored in the input."
msgstr "Informationen über die Datensatztypen in der Standardeingabe ausgeben."

#. type: TP
#: debian-buster
#, no-wrap
msgid "B<-t>, B<--type>=I<RECORD_TYPE>"
msgstr "B<-t>, B<--type>=I<DATENSATZTYP>"

#. type: Plain text
#: debian-buster debian-unstable fedora-rawhide
msgid "print information on the records having the specified type."
msgstr "Informationen zu Datensätzen des angegebenen Typs ausgeben."

#. type: TP
#: debian-buster debian-unstable fedora-rawhide
#, no-wrap
msgid "B<-d>, B<--descriptor>"
msgstr "B<-d>, B<--descriptor>"

#. type: Plain text
#: debian-buster debian-unstable fedora-rawhide
msgid "include the full record descriptors."
msgstr "Vollständige Datensatzdeskriptoren einschließen."

#. type: TP
#: debian-buster debian-unstable fedora-rawhide
#, no-wrap
msgid "B<-n>, B<--names-only>"
msgstr "B<-n>, B<--names-only>"

#. type: Plain text
#: debian-buster debian-unstable fedora-rawhide
msgid "output just the names of the record files found in the input."
msgstr "Nur die Namen der in der Eingabe gefundenen Datensatzdateien ausgeben."

#. type: TP
#: debian-buster debian-unstable fedora-rawhide
#, no-wrap
msgid "B<--help>"
msgstr "B<--help>"

#. type: Plain text
#: debian-buster debian-unstable fedora-rawhide
msgid "print a help message and exit."
msgstr "Hilfe ausgeben und beenden."

#. type: TP
#: debian-buster debian-unstable fedora-rawhide
#, no-wrap
msgid "B<--version>"
msgstr "B<--version>"

#. type: Plain text
#: debian-buster debian-unstable fedora-rawhide
msgid "show version and exit."
msgstr "Version anzeigen und beenden."

#. type: SS
#: debian-buster debian-unstable fedora-rawhide
#, no-wrap
msgid "Special options:"
msgstr "Spezielle Optionen:"

#. type: TP
#: debian-buster debian-unstable fedora-rawhide
#, no-wrap
msgid "B<-S>, B<--print-sexps>"
msgstr "B<-S>, B<--print-sexps>"

#. type: Plain text
#: debian-buster debian-unstable fedora-rawhide
msgid "print the data in sexps instead of rec format."
msgstr "Daten im SEXPS- anstelle des REC-Formats ausgeben."

#. type: SH
#: debian-buster debian-unstable fedora-rawhide
#, no-wrap
msgid "AUTHOR"
msgstr "AUTOR"

#. type: Plain text
#: debian-buster debian-unstable fedora-rawhide
msgid "Written by Jose E. Marchesi."
msgstr "Geschrieben von Jose E. Marchesi."

#. type: SH
#: debian-buster debian-unstable fedora-rawhide
#, no-wrap
msgid "REPORTING BUGS"
msgstr "FEHLER MELDEN"

#. type: Plain text
#: debian-buster debian-unstable fedora-rawhide
msgid "Report bugs to: bug-recutils@gnu.org"
msgstr "Melden Sie Fehler (auf Englisch) an bug-recutils@gnu.org"

#. type: Plain text
#: debian-buster debian-unstable fedora-rawhide
msgid "GNU recutils home page: E<lt>http://www.gnu.org/software/recutils/E<gt>"
msgstr ""
"Website der GNU Recutils: E<lt>http://www.gnu.org/software/recutils/E<gt>"

#. type: Plain text
#: debian-buster debian-unstable fedora-rawhide
msgid "General help using GNU software: E<lt>http://www.gnu.org/gethelp/E<gt>"
msgstr ""
"Allgemeine Hilfe zur Benutzung von GNU-Software: E<lt>http://www.gnu.org/"
"gethelp/E<gt>"


#. type: Plain text
#: fedora-36
msgid ""
"B<grub-mkstandalone> \\(em Generate a standalone image in the selected "
"format."
msgstr ""
"B<grub-mkstandalone> \\(em ein eigenständiges Abbild im angegebenen Format "
"erzeugen"

#. type: SH
#: fedora-36 fedora-rawhide mageia-cauldron opensuse-leap-15-4
#: opensuse-tumbleweed
#, no-wrap
msgid "SYNOPSIS"
msgstr "ÜBERSICHT"

#. type: Plain text
#: fedora-36
msgid "B<grub-mkstandalone> [-o | --output=I<FILE>] [-O | --format=I<FORMAT>]"
msgstr ""
"B<grub-mkstandalone> [-o | --output=I<DATEI>] [-O | --format=I<FORMAT>]"

#. type: Plain text
#: fedora-36
msgid "[-C | --compression=(I<xz>|I<none>|I<auto>)]"
msgstr "[-C | --compression=(I<xz>|I<none>|I<auto>)]"

#. type: Plain text
#: fedora-36
msgid "[--modules=I<MODULES>] [--install-modules=I<MODULES>]"
msgstr "[--modules=I<MODULE>] [--install-modules=I<MODULE>]"

#. type: Plain text
#: fedora-36
msgid "[--themes=I<THEMES>] [--fonts=I<FONTS>]"
msgstr "[--themes=I<THEMEN>] [--fonts=I<SCHRIFTEN>]"

#. type: Plain text
#: fedora-36
msgid "[--locales=I<LOCALES>] [--compress[=I<no>,I<xz>,I<gz>,I<lzo>]]"
msgstr "[--locales=I<LOCALES>] [--compress[=I<no>,I<xz>,I<gz>,I<lzo>]]"


#. type: TH
#: debian-bullseye debian-unstable fedora-37 fedora-rawhide
#, no-wrap
msgid "RECSEL"
msgstr "RECSEL"

#. type: TH
#: debian-bullseye
#, no-wrap
msgid "January 2019"
msgstr "Januar 2019"

#. type: TH
#: debian-bullseye
#, no-wrap
msgid "recsel 1.8"
msgstr "recsel 1.8"

#. type: TH
#: debian-bullseye debian-unstable fedora-37 fedora-rawhide
#, no-wrap
msgid "User Commands"
msgstr "Dienstprogramme für Benutzer"

#. type: SH
#: debian-bullseye debian-unstable fedora-37 fedora-rawhide
#, no-wrap
msgid "NAME"
msgstr "BEZEICHNUNG"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-37 fedora-rawhide
msgid "recsel - print records from a recfile"
msgstr "recsel - Datensätze aus einer REC-Datei ausgeben"

#. type: SH
#: debian-bullseye debian-unstable fedora-37 fedora-rawhide
#, no-wrap
msgid "SYNOPSIS"
msgstr "ÜBERSICHT"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-37 fedora-rawhide
msgid ""
"B<recsel> [I<\\,OPTION\\/>]... [I<\\,-t TYPE\\/>] [I<\\,-j FIELD\\/>] [I<\\,-"
"n INDEXES | -e RECORD_EXPR | -q STR | -m NUM\\/>] [I<\\,-c | (-p|-P) "
"FIELD_EXPR\\/>] [I<\\,FILE\\/>]..."
msgstr ""
"B<recsel> [I<\\,OPTION\\/>] … [I<\\,-t TYP\\/>] [I<\\,-j FELD\\/>] [I<\\,-n "
"INDIZES | -e AUSW_AUSDR | -q ZEICHENKETTE | -m ZAHL\\/>] [I<\\,-c | (-p|-P) "
"FELD_AUSDR\\/>] [I<\\,DATEI\\/>] …"

#. type: SH
#: debian-bullseye debian-unstable fedora-37 fedora-rawhide
#, no-wrap
msgid "DESCRIPTION"
msgstr "BESCHREIBUNG"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-37 fedora-rawhide
msgid "Select and print rec data."
msgstr "REC-Daten auswählen und ausgeben."

#. type: TP
#: debian-bullseye debian-unstable fedora-37 fedora-rawhide
#, no-wrap
msgid "B<-d>, B<--include-descriptors>"
msgstr "B<-d>, B<--include-descriptors>"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-37 fedora-rawhide
msgid "print record descriptors along with the matched records."
msgstr "Datensatzdeskriptoren mit den übereinstimmenden Datensätzen ausgeben."

#. type: TP
#: debian-bullseye debian-unstable fedora-37 fedora-rawhide
#, no-wrap
msgid "B<-C>, B<--collapse>"
msgstr "B<-C>, B<--collapse>"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-37 fedora-rawhide
msgid "do not section the result in records with newlines."
msgstr "Ergebnis in Datensätzen nicht in neue Zeile umbrechen."

#. type: TP
#: debian-bullseye debian-unstable fedora-37 fedora-rawhide
#, no-wrap
msgid "B<-S>, B<--sort>=I<\\,FIELD\\/>,..."
msgstr "B<-S>, B<--sort>=I<\\,FELD\\/>, …"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-37 fedora-rawhide
msgid "sort the output by the specified fields."
msgstr "Ausgabe anhand der angegebenen Felder sortieren."

#. type: TP
#: debian-bullseye debian-unstable fedora-37 fedora-rawhide
#, no-wrap
msgid "B<-G>, B<--group-by>=I<\\,FIELD\\/>,..."
msgstr "B<-G>, B<--group-by>=I<\\,FELD\\/>, …"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-37 fedora-rawhide
msgid "group records by the specified fields."
msgstr "Datensätze anhand der angegebenen Felder gruppieren."

#. type: TP
#: debian-bullseye debian-unstable fedora-37 fedora-rawhide
#, no-wrap
msgid "B<-U>, B<--uniq>"
msgstr "B<-U>, B<--uniq>"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-37 fedora-rawhide
msgid "remove duplicated fields in the output records."
msgstr "Feldduplikate in ausgegebenen Datensätzen entfernen."

#. type: TP
#: debian-bullseye debian-unstable fedora-37 fedora-rawhide
#, no-wrap
msgid "B<-s>, B<--password>=I<\\,STR\\/>"
msgstr "B<-s>, B<--password>=I<\\,ZEICHENKETTE\\/>"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-37 fedora-rawhide
msgid "decrypt confidential fields with the given password."
msgstr "Vertrauliche Felder mit dem angegebenen Passwort entschlüsseln."

#. type: TP
#: debian-bullseye debian-unstable fedora-37 fedora-rawhide
#, no-wrap
msgid "B<--help>"
msgstr "B<--help>"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-37 fedora-rawhide
msgid "print a help message and exit."
msgstr "Hilfe ausgeben und beenden."

#. type: TP
#: debian-bullseye debian-unstable fedora-37 fedora-rawhide
#, no-wrap
msgid "B<--version>"
msgstr "B<--version>"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-37 fedora-rawhide
msgid "show version and exit."
msgstr "Version anzeigen und beenden."

#. type: SS
#: debian-bullseye debian-unstable fedora-37 fedora-rawhide
#, no-wrap
msgid "Record selection options:"
msgstr "Optionen zur Datensatzauswahl:"

#. type: TP
#: debian-bullseye debian-unstable fedora-37 fedora-rawhide
#, no-wrap
msgid "B<-i>, B<--case-insensitive>"
msgstr "B<-i>, B<--case-insensitive>"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-37 fedora-rawhide
msgid "make strings case-insensitive in selection expressions."
msgstr "Groß- oder Kleinschreibung in Auswahlausdrücken nicht beachten."

#. type: TP
#: debian-bullseye debian-unstable fedora-37 fedora-rawhide
#, no-wrap
msgid "B<-t>, B<--type>=I<\\,TYPE\\/>"
msgstr "B<-t>, B<--type>=I<\\,TYP\\/>"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-37 fedora-rawhide
msgid "operate on records of the specified type only."
msgstr "Nur auf Datensätze des angegebenen Typs anwenden."

#. type: TP
#: debian-bullseye debian-unstable fedora-37 fedora-rawhide
#, no-wrap
msgid "B<-e>, B<--expression>=I<\\,RECORD_EXPR\\/>"
msgstr "B<-e>, B<--expression>=I<\\,DATENS_AUSDR\\/>"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-37 fedora-rawhide
msgid "selection expression."
msgstr "Auswahlausdruck."

#. type: TP
#: debian-bullseye debian-unstable fedora-37 fedora-rawhide
#, no-wrap
msgid "B<-q>, B<--quick>=I<\\,STR\\/>"
msgstr "B<-q>, B<--quick>=I<\\,ZEICHENKETTE\\/>"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-37 fedora-rawhide
msgid "select records with fields containing a string."
msgstr "Datensätze mit Feldern auswählen, die eine Zeichenkette enthalten."

#. type: TP
#: debian-bullseye debian-unstable fedora-37 fedora-rawhide
#, no-wrap
msgid "B<-n>, B<--number>=I<\\,NUM\\/>,..."
msgstr "B<-n>, B<--number>=I<\\,ZAHL\\/>,…"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-37 fedora-rawhide
msgid "select specific records by position, with ranges."
msgstr "Bestimmte Datensätze nach Position auswählen, mit Bereichsangabe."

#. type: TP
#: debian-bullseye debian-unstable fedora-37 fedora-rawhide
#, no-wrap
msgid "B<-m>, B<--random>=I<\\,NUM\\/>"
msgstr "B<-m>, B<--random>=I<\\,ZAHL\\/>"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-37 fedora-rawhide
msgid "select a given number of random records."
msgstr "Eine angegebene Zahl von Datensätzen zufällig auswählen."

#. type: TP
#: debian-bullseye debian-unstable fedora-37 fedora-rawhide
#, no-wrap
msgid "B<-j>, B<--join>=I<\\,FIELD\\/>"
msgstr "B<-j>, B<--join>=I<\\,FELD\\/>"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-37 fedora-rawhide
msgid "perform an inner join using the specified field."
msgstr "Eine innere Verknüpfung mit dem angegebenen Feld ausführen."

#. type: SS
#: debian-bullseye debian-unstable fedora-37 fedora-rawhide
#, no-wrap
msgid "Output options:"
msgstr "Ausgabeoptionen:"

#. type: TP
#: debian-bullseye debian-unstable fedora-37 fedora-rawhide
#, no-wrap
msgid "B<-p>, B<--print>=I<\\,FIELDS\\/>"
msgstr "B<-p>, B<--print>=I<\\,FELDER\\/>"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-37 fedora-rawhide
msgid "comma-separated list of fields to print for each matching record."
msgstr ""
"Kommagetrennte Liste von Feldern, die für jeden  übereinstimmenden Datensatz "
"auszugeben sind."

#. type: TP
#: debian-bullseye debian-unstable fedora-37 fedora-rawhide
#, no-wrap
msgid "B<-P>, B<--print-values>=I<\\,FIELDS\\/>"
msgstr "B<-P>, B<--print-values>=I<\\,FELDER\\/>"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-37 fedora-rawhide
msgid "as B<-p>, but print only the values of the selected fields."
msgstr "WieB<-p>, aber die Werte der ausgewählten Felder ausgeben."

#. type: TP
#: debian-bullseye debian-unstable fedora-37 fedora-rawhide
#, no-wrap
msgid "B<-R>, B<--print-row>=I<\\,FIELDS\\/>"
msgstr "B<-R>, B<--print-row>=I<\\,FELDER\\/>"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-37 fedora-rawhide
msgid "as B<-P>, but separate the values with spaces instead of newlines."
msgstr ""
"Wie B<-P>, aber ausgegebene Werte durch Leerzeichen anstatt durch "
"Zeilenumbrüche trennen."

#. type: TP
#: debian-bullseye debian-unstable fedora-37 fedora-rawhide
#, no-wrap
msgid "B<-c>, B<--count>"
msgstr "B<-c>, B<--count>"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-37 fedora-rawhide
msgid ""
"print a count of the matching records instead of the records themselves."
msgstr ""
"Anzahl der übereinstimmenden Datensätze anstelle der Datensätze selbst "
"ausgeben."

#. type: SS
#: debian-bullseye debian-unstable fedora-37 fedora-rawhide
#, no-wrap
msgid "Special options:"
msgstr "Spezielle Optionen:"

#. type: TP
#: debian-bullseye debian-unstable fedora-37 fedora-rawhide
#, no-wrap
msgid "B<--print-sexps>"
msgstr "B<-S>, B<--print-sexps>"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-37 fedora-rawhide
msgid "print the data in sexps instead of rec format."
msgstr "Daten im SEXPS- anstelle des REC-Formats ausgeben."


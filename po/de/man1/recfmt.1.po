#, no-wrap
msgid "January 2019"
msgstr "Januar 2019"

#. type: TH
#: debian-bullseye debian-unstable fedora-rawhide
#, no-wrap
msgid "recfmt 1.8"
msgstr "recfmt 1.8"

#. type: TH
#: debian-bullseye debian-unstable fedora-rawhide
#, no-wrap
msgid "User Commands"
msgstr "Dienstprogramme für Benutzer"

#. type: SH
#: debian-bullseye debian-unstable fedora-rawhide
#, no-wrap
msgid "NAME"
msgstr "BEZEICHNUNG"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-rawhide
msgid "recfmt - apply a template to records"
msgstr "recfmt - eine Vorlage auf Datensätze anwenden"

#. type: SH
#: debian-bullseye debian-unstable fedora-rawhide
#, no-wrap
msgid "SYNOPSIS"
msgstr "ÜBERSICHT"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-rawhide
msgid "B<recfmt> [I<\\,OPTION\\/>]... [I<\\,TEMPLATE\\/>]"
msgstr "B<recfmt> [I<\\,OPTION\\/>] … [I<\\,VORLAGE\\/>]"

#. type: SH
#: debian-bullseye debian-unstable fedora-rawhide
#, no-wrap
msgid "DESCRIPTION"
msgstr "BESCHREIBUNG"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-rawhide
msgid "Apply a template to records read from standard input."
msgstr ""
"Eine Vorlage auf die aus der Standardeingabe gelesenen Datensätze anwenden."

#. type: TP
#: debian-bullseye debian-unstable fedora-rawhide
#, no-wrap
msgid "B<-f>, B<--file>=I<\\,FILENAME\\/>"
msgstr "B<-f>, B<--file>=I<\\,DATEINAME\\/>"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-rawhide
msgid "read the template to apply from a file."
msgstr "Zu benutzende Vorlage aus einer Datei holen."

#. type: TP
#: debian-bullseye debian-unstable fedora-rawhide
#, no-wrap
msgid "B<--help>"
msgstr "B<--help>"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-rawhide
msgid "print a help message and exit."
msgstr "Hilfe ausgeben und beenden."

#. type: TP
#: debian-bullseye debian-unstable fedora-rawhide
#, no-wrap
msgid "B<--version>"
msgstr "B<--version>"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-rawhide
msgid "show version and exit."
msgstr "Version anzeigen und beenden."

#. type: SH
#: debian-bullseye debian-unstable fedora-rawhide
#, no-wrap
msgid "AUTHOR"
msgstr "AUTOR"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-rawhide
msgid "Written by Jose E. Marchesi."
msgstr "Geschrieben von Jose E. Marchesi."

#. type: SH
#: debian-bullseye debian-unstable fedora-rawhide
#, no-wrap
msgid "REPORTING BUGS"
msgstr "FEHLER MELDEN"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-rawhide
msgid "Report bugs to: bug-recutils@gnu.org"
msgstr "Melden Sie Fehler (auf Englisch) an bug-recutils@gnu.org"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-rawhide
msgid "GNU recutils home page: E<lt>http://www.gnu.org/software/recutils/E<gt>"
msgstr ""

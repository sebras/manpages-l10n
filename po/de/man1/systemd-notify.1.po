#: debian-bullseye fedora-37 opensuse-tumbleweed
#, no-wrap
msgid "systemd 251"
msgstr "systemd 251"

#. type: TH
#: opensuse-leap-15-5
#, no-wrap
msgid "systemd 249"
msgstr "systemd 249"

#. type: Plain text
#: opensuse-leap-15-5
#, no-wrap
msgid "#!/bin/bash\n"
msgstr "#!/bin/bash\n"

#. type: Plain text
#: opensuse-leap-15-5
#, no-wrap
msgid ""
"while : ; do\n"
"        read a E<lt> /tmp/waldo\n"
"        systemd-notify --status=\"Processing $a\"\n"
msgstr ""
"while : ; do\n"
"        read a E<lt> /tmp/waldo\n"
"        systemd-notify --status=\"Verarbeite $a\"\n"

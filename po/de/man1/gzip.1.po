"      #! /bin/sh\n"
"      export PATH=/usr/bin\n"
"      exec gzip -9 \"$@\"\n"
msgstr ""
"      #! /bin/sh\n"
"      export PATH=/usr/bin\n"
"      exec gzip -9 \"$@\"\n"


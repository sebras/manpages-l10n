"$ file -i Datei.c file /dev/{wd0a,hda}\n"
"file.c:      text/x-c\n"
"file:        application/x-executable\n"
"/dev/hda:    application/x-not-regular-file\n"
"/dev/wd0a:   application/x-not-regular-file\n"

#. type: Plain text

#: opensuse-leap-15-4
#, no-wrap
msgid "To see every process on the system using standard syntax:"
msgstr "Alle Prozesse im System in der Standard-Syntax anzeigen:"

#. type: Plain text
#: opensuse-leap-15-4
msgid "B<ps\\ -e>"
msgstr "B<ps\\ -e>"

#. type: Plain text
#: opensuse-leap-15-4
msgid "B<ps\\ -ef>"
msgstr "B<ps\\ -ef>"

#. type: Plain text
#: opensuse-leap-15-4
msgid "B<ps\\ -eF>"
msgstr "B<ps\\ -eF>"

#. type: Plain text
#: opensuse-leap-15-4
msgid "B<ps\\ -ely>"
msgstr "B<ps\\ -ely>"

#. type: TP
#: opensuse-leap-15-4
#, no-wrap
msgid "To see every process on the system using BSD syntax:"
msgstr "Alle Prozess im System in der BSD-Syntax anzeigen:"

#. type: Plain text
#: opensuse-leap-15-4
msgid "B<ps\\ ax>"
msgstr "B<ps\\ ax>"

#. type: Plain text
#: opensuse-leap-15-4
msgid "B<ps\\ axu>"
msgstr "B<ps\\ axu>"


#. type: Plain text
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
msgid "rec2csv - rec to csv converter"
msgstr "csv2rec - Konverter von REC nach CSV"

#. type: SH
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
#, no-wrap
msgid "SYNOPSIS"
msgstr "ÜBERSICHT"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
msgid "B<rec2csv> [I<\\,OPTIONS\\/>]... [I<\\,REC_FILE\\/>]"
msgstr "B<rec2csv> [I<\\,OPTIONEN\\/>] … [I<\\,REC-DATEI\\/>]"

#. type: SH
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
#, no-wrap
msgid "DESCRIPTION"
msgstr "BESCHREIBUNG"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
msgid "Convert rec data into csv data."
msgstr "REC-Daten in CSV-Daten umwandeln."

#. type: TP
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
#, no-wrap
msgid "B<-d>, B<--delim>=I<\\,char\\/>"
msgstr "B<-d>, B<--delim>=I<\\,Zeichen\\/>"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
msgid "sets the deliminator (default ',')"
msgstr "legt das Trennzeichen fest (Voreinstellung »,«)"

#. type: TP
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
#, no-wrap
msgid "B<-t>, B<--type>=I<\\,TYPE\\/>"
msgstr "B<-t>, B<--type>=I<\\,TYP\\/>"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
msgid ""
"record set to convert to csv; if this parameter is omitted then the default "
"record set is used"
msgstr ""
"Datensatz zum Umwandeln in CSV; Falls dieser Parameter fehlt, wird der "
"vorgegebene Datensatz verwendet."

#. type: TP
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
#, no-wrap
msgid "B<-S>, B<--sort>=I<\\,FIELDS\\/>"
msgstr "B<-S>, B<--sort>=I<\\,FELDER\\/>"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
msgid "sort the output by the specified fields."
msgstr "Ausgabe anhand der angegebenen Felder sortieren."

#. type: TP
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
#, no-wrap
msgid "B<--help>"
msgstr "B<--help>"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
msgid "print a help message and exit."
msgstr "Hilfe ausgeben und beenden."

#. type: TP
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
#, no-wrap
msgid "B<--version>"
msgstr "B<--version>"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
msgid "show version and exit."
msgstr "Version anzeigen und beenden."


msgid "SEE ALSO"
msgstr "SIEHE AUCH"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-37 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-5 opensuse-tumbleweed
msgid ""
"B<awk>(1), B<ed>(1), B<grep>(1), B<tr>(1), B<perlre>(1), sed.info, any of "
"various books on \\*(sd,"
msgstr ""

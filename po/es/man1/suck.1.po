#: debian-bullseye debian-unstable
msgid "QUOTE=c"
msgstr "QUOTE=c"

#. type: Plain text
#: debian-bullseye debian-unstable
msgid "NON_REGEX=c"
msgstr "NON_REGEX=c"

#. type: Plain text
#: debian-bullseye debian-unstable
msgid "GROUP=keep groupname filename OR GROUP=delete groupname filename"
msgstr "GROUP=keep grupo archivo O BIEN GROUP=delete grupo archivo"

#. type: Plain text
#: debian-bullseye debian-unstable

msgid ""
"int\n"
"main(void)\n"
"{\n"
"    char buf[BUFSIZ];\n"
"    setbuf(stdin, buf);\n"
"    printf(\"Hello, world!\\en\");\n"
"    return 0;\n"
"}\n"
msgstr ""
"int\n"
"main(void)\n"
"{\n"
"    auto char buf[BUFSIZ];\n"
"    setbuf(stdin, buf);\n"
"    printf(\"¡Hola a todos!\\en\");\n"
"    return 0; /* Se destruye buf, se cierra stdin */\n"
"}\n"


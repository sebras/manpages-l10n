msgstr ""
"int\n"
"main(void)\n"
"{\n"
"    char key[64];\n"
"    char orig[9] = \"eggplant\";\n"
"    char buf[64];\n"
"    char txt[9];\n"
"    int i, j;\n"

#. type: Plain text
#: opensuse-leap-15-3
#, no-wrap
msgid ""
"    for (i = 0; i E<lt> 64; i++) {\n"
"        key[i] = rand() & 1;\n"
"    }\n"
msgstr ""
"    for (i = 0; i E<lt> 64; i++) {\n"
"        key[i] = rand() & 1;\n"
"    }\n"

#. type: Plain text
#: opensuse-leap-15-3
#, no-wrap
msgid ""
"    for (i = 0; i E<lt> 8; i++) {\n"
"        for (j = 0; j E<lt> 8; j++) {\n"
"            buf[i * 8 + j] = orig[i] E<gt>E<gt> j & 1;\n"
"        }\n"
"        setkey(key);\n"
"    }\n"
"    printf(\"Before encrypting: %s\\en\", orig);\n"
msgstr ""
"    for (i = 0; i E<lt> 8; i++) {\n"
"        for (j = 0; j E<lt> 8; j++) {\n"
"            buf[i * 8 + j] = orig[i] E<gt>E<gt> j & 1;\n"
"        }\n"
"        setkey(key);\n"
"    }\n"
"    printf(\"Before encrypting: %s\\en\", orig);\n"

#. type: Plain text
#: opensuse-leap-15-3
#, no-wrap
msgid ""
"    encrypt(buf, 0);\n"
"    for (i = 0; i E<lt> 8; i++) {\n"
"        for (j = 0, txt[i] = \\(aq\\e0\\(aq; j E<lt> 8; j++) {\n"

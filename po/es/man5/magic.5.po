msgid ""
"0      string   MZ\n"
"\\*[Gt]0x18  leshort  \\*[Lt]0x40   MS-DOS executable\n"
"\\*[Gt]0x18  leshort  \\*[Gt]0x3f   extended PC executable (e.g., MS Windows)\n"
msgstr ""

#. type: Plain text

#, no-wrap
msgid ""
"  :0 Bh\n"
"  *  20^1 ^E<gt>\n"
"  * -10^1 ^[^E<gt>]\n"
"  /dev/null\n"
msgstr ""
"  :0 Bh\n"
"  *  20^1 ^E<gt>\n"
"  * -10^1 ^[^E<gt>]\n"
"  /dev/null\n"


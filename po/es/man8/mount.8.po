#: archlinux fedora-rawhide mageia-cauldron
#, fuzzy
#| msgid ""
#| "B<mount -t tmpfs none /mnt -o \\e>\n"
#| "B<\\ \\ 'context=\"system_u:object_r:tmp_t:s0:c127,c456\",noexec'>\n"
msgid ""
"mount -t tmpfs none /mnt -o \\(rs \\(aqcontext=\"system_u:object_r:tmp_t:s0:"
"c127,c456\",noexec\\(aq"
msgstr ""
"B<mount -t tmpfs none /mnt -o \\e>\n"
"B<\\ \\ 'context=\"system_u:object_r:tmp_t:s0:c127,c456\",noexec'>\n"


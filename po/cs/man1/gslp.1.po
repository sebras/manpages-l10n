msgstr "Přednastavená zařízení (-sDEVICE=) a rozlišení (-r) jsou:"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable mageia-cauldron
#: opensuse-tumbleweed
#, no-wrap
msgid ""
"      gslp      epson      180\n"
"      gsbj      bj10e      180\n"
"      gsdj      deskjet    300\n"
"      gsdj500   djet500    300\n"
"      gslj      laserjet   300\n"
msgstr ""
"      gslp      epson      180\n"
"      gsbj      bj10e      180\n"
"      gsdj      deskjet    300\n"
"      gsdj500   djet500    300\n"
"      gslj      laserjet   300\n"


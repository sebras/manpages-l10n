msgid "SYNOPSIS"
msgstr "POUŽITÍ"

#. type: Plain text
#: archlinux fedora-37 fedora-rawhide mageia-cauldron opensuse-tumbleweed
msgid "B<gs> [ I<options> ] [ I<files> ] ... B<(Unix, VMS)>"
msgstr "B<gs> [ I<volby> ] [ I<soubory> ] ... B<(Unix, VMS)>"

#. type: Plain text
#: archlinux fedora-37 fedora-rawhide mageia-cauldron opensuse-tumbleweed
msgid "B<gswin32c> [ I<options> ] [ I<files> ] ... B<(MS Windows)>"
msgstr "B<gswin32c> [ I<volby> ] [ I<soubory> ] ... B<(MS Windows)>"

#. type: Plain text
#: archlinux fedora-37 fedora-rawhide mageia-cauldron opensuse-tumbleweed

msgid ""
"B<saned> B<[ -a> I<[ username ]> B<]> B<[ -u> I<username> B<]> B<[ -b> "
"I<address> B<]> B<[ -p> I<port> B<]> B<[ -l ]> B<[ -D ]> B<[ -o ]> B<[ -d> "
"I<n> B<]> B<[ -e ]> B<[ -h ]>"
msgstr ""


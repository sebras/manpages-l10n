msgid "SYNOPSIS"
msgstr "ÖSSZEGZÉS"

#. type: Plain text
#: archlinux fedora-36 fedora-rawhide mageia-cauldron
#, fuzzy
#| msgid ""
#| "B<chfn> [\\ -f\\ full-name\\ ] [\\ -o\\ office\\ ] [\\ -p\\ office-phone"
#| "\\ ] [\\ -h\\ home-phone\\ ] [\\ -u\\ ] [\\ -v\\ ] [\\ username\\ ]"
msgid ""
"B<chfn> [B<-f> I<full-name>] [B<-o> I<office>] [B<-p> I<office-phone>] [B<-"
"h> I<home-phone>] [B<-u>] [B<-v>] [I<username>]"
msgstr ""
"B<chfn> [\\ -f\\ teljes név\\ ] [\\ -o\\ iroda\\ ] [\\ -p\\ irodai telefon"
"\\ ] [\\ -h\\ otthoni telefon\\ ] [\\ -u\\ ] [\\ -v\\ ] [\\ felhasználó\\ ]"


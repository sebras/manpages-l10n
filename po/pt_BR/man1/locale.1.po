"tel_dom_fmt=\"(%a) %l\"\n"
"int_select=\"11\"\n"
"int_prefix=\"1\"\n"
"telephone-codeset=\"UTF-8\"\n"
msgstr ""
"$ B<locale -k LC_TELEPHONE>\n"
"tel_int_fmt=\"+%c (%a) %l\"\n"
"tel_dom_fmt=\"(%a) %l\"\n"
"int_select=\"11\"\n"

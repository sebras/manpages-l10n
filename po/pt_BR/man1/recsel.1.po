#. type: TH
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
#, no-wrap
msgid "RECSEL"
msgstr "RECSEL"

#. type: TH
#: debian-bullseye debian-unstable fedora-36
#, no-wrap
msgid "January 2019"
msgstr "Janeiro de 2019"

#. type: TH
#: debian-bullseye debian-unstable fedora-36
#, no-wrap
msgid "recsel 1.8"
msgstr "recsel 1.8"

#. type: TH
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
#, no-wrap
msgid "User Commands"
msgstr "Comandos de usuário"

#. type: SH
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
#, no-wrap
msgid "NAME"
msgstr "NOME"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
msgid "recsel - print records from a recfile"
msgstr "recsel - imprime registros de um arquivo rec"

#. type: SH
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
#, no-wrap
msgid "SYNOPSIS"
msgstr "SINOPSE"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
msgid ""
"B<recsel> [I<\\,OPTION\\/>]... [I<\\,-t TYPE\\/>] [I<\\,-j FIELD\\/>] [I<\\,-"
"n INDEXES | -e RECORD_EXPR | -q STR | -m NUM\\/>] [I<\\,-c | (-p|-P) "
"FIELD_EXPR\\/>] [I<\\,FILE\\/>]..."
msgstr ""
"B<recsel> [I<\\,OPÇÃO\\/>]... [I<\\,-t TIPO\\/>] [I<\\,-j CAMPO\\/>] [I<\\,-"
"n ÍNDICES | -e EXPR_REGISTRO | -q TEXTO | -m NÚM\\/>] [I<\\,-c | (-p|-P) "
"EXPR_CAMPO\\/>] [I<\\,ARQUIVO\\/>]..."

#. type: SH
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
#, no-wrap
msgid "DESCRIPTION"
msgstr "DESCRIÇÃO"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
msgid "Select and print rec data."
msgstr "Selecione e imprime dados rec."

#. type: TP
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
#, no-wrap
msgid "B<-d>, B<--include-descriptors>"
msgstr "B<-d>, B<--include-descriptors>"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
msgid "print record descriptors along with the matched records."
msgstr ""
"imprime descritores de registro junto com os registros correspondentes."

#. type: TP
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
#, no-wrap
msgid "B<-C>, B<--collapse>"
msgstr "B<-C>, B<--collapse>"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
msgid "do not section the result in records with newlines."
msgstr "não secciona o resultado em registros com novas linhas."

#. type: TP
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
#, no-wrap
msgid "B<-S>, B<--sort>=I<\\,FIELD\\/>,..."
msgstr "B<-S>, B<--sort>=I<\\,CAMPO\\/>,..."

#. type: Plain text
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
msgid "sort the output by the specified fields."
msgstr "ordena a saída pelos campos especificados."

#. type: TP
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
#, no-wrap
msgid "B<-G>, B<--group-by>=I<\\,FIELD\\/>,..."
msgstr "B<-G>, B<--group-by>=I<\\,CAMPO\\/>,..."

#. type: Plain text
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
msgid "group records by the specified fields."
msgstr "agrupa registros pelos campos especificados."

#. type: TP
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
#, no-wrap
msgid "B<-U>, B<--uniq>"
msgstr "B<-U>, B<--uniq>"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
msgid "remove duplicated fields in the output records."
msgstr "remove campos duplicados nos registros de saída."

#. type: TP
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
#, no-wrap
msgid "B<-s>, B<--password>=I<\\,STR\\/>"
msgstr "B<-s>, B<--password>=I<\\,TEXTO\\/>"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
msgid "decrypt confidential fields with the given password."
msgstr "descriptografa campos confidenciais com a SENHA."

#. type: TP
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
#, no-wrap
msgid "B<--help>"
msgstr "B<--help>"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
msgid "print a help message and exit."
msgstr "mostra uma mensagem de ajuda e sai."

#. type: TP
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
#, no-wrap
msgid "B<--version>"
msgstr "B<--version>"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
msgid "show version and exit."
msgstr "mostra a versão e sai."

#. type: SS
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
#, no-wrap
msgid "Record selection options:"
msgstr "Opções de seleção de registros:"

#. type: TP
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
#, no-wrap
msgid "B<-i>, B<--case-insensitive>"
msgstr "B<-i>, B<--case-insensitive>"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
msgid "make strings case-insensitive in selection expressions."
msgstr ""
"diferencia textos entre maiúsculo e minúsculo nas expressões de seleção."

#. type: TP
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
#, no-wrap
msgid "B<-t>, B<--type>=I<\\,TYPE\\/>"
msgstr "B<-t>, B<--type>=I<\\,TIPO\\/>"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
msgid "operate on records of the specified type only."
msgstr "opera em registros apenas do tipo especificado."

#. type: TP
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
#, no-wrap
msgid "B<-e>, B<--expression>=I<\\,RECORD_EXPR\\/>"
msgstr "B<-e>, B<--expression>=I<\\,EXPR_REGISTRO\\/>"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
msgid "selection expression."
msgstr "expressão de seleção."

#. type: TP
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
#, no-wrap
msgid "B<-q>, B<--quick>=I<\\,STR\\/>"
msgstr "B<-q>, B<--quick>=I<\\,TEXTO\\/>"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
msgid "select records with fields containing a string."
msgstr "seleciona registros com campos contendo um texto."

#. type: TP
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
#, no-wrap
msgid "B<-n>, B<--number>=I<\\,NUM\\/>,..."
msgstr "B<-n>, B<--number>=I<\\,NÚM\\/>,..."

#. type: Plain text
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
msgid "select specific records by position, with ranges."
msgstr "seleciona registros por posição, com intervalos."

#. type: TP
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
#, no-wrap
msgid "B<-m>, B<--random>=I<\\,NUM\\/>"
msgstr "B<-m>, B<--random>=I<\\,NÚM\\/>"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
msgid "select a given number of random records."
msgstr "seleciona um número dado de registros aleatórios."

#. type: TP
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
#, no-wrap
msgid "B<-j>, B<--join>=I<\\,FIELD\\/>"
msgstr "B<-j>, B<--join>=I<\\,CAMPO\\/>"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
msgid "perform an inner join using the specified field."
msgstr "faz uma aglutinação, estilo inner join, usando o campo especificado."

#. type: SS
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
#, no-wrap
msgid "Output options:"
msgstr "Opções de saída:"

#. type: TP
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
#, no-wrap
msgid "B<-p>, B<--print>=I<\\,FIELDS\\/>"
msgstr "B<-p>, B<--print>=I<\\,CAMPOS\\/>"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
msgid "comma-separated list of fields to print for each matching record."
msgstr ""
"lista separada por vírgula de campos a ser impressos para cada registro "
"correspondente."

#. type: TP
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
#, no-wrap
msgid "B<-P>, B<--print-values>=I<\\,FIELDS\\/>"
msgstr "B<-P>, B<--print-values>=I<\\,CAMPOS\\/>"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
msgid "as B<-p>, but print only the values of the selected fields."
msgstr ""
"semelhante a B<-p>, mas imprime apenas os valores dos campos selecionados."

#. type: TP
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
#, no-wrap
msgid "B<-R>, B<--print-row>=I<\\,FIELDS\\/>"
msgstr "B<-R>, B<--print-row>=I<\\,CAMPOS\\/>"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
msgid "as B<-P>, but separate the values with spaces instead of newlines."
msgstr ""
"semelhante a B<-P>, mas separa os valores com espaços, ao invés de novas "
"linhas."

#. type: TP
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
#, no-wrap
msgid "B<-c>, B<--count>"
msgstr "B<-c>, B<--count>"

#. type: Plain text
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
msgid ""
"print a count of the matching records instead of the records themselves."
msgstr ""
"imprime uma contagem de registros correspondentes, ao invés dos registros em "
"si."

#. type: SS
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
#, no-wrap
msgid "Special options:"
msgstr "Opções especiais:"

#. type: TP
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
#, no-wrap
msgid "B<--print-sexps>"
msgstr "B<--print-sexps>"

# Alinhado com outras mensagens de saída do --help
#. type: Plain text
#: debian-bullseye debian-unstable fedora-36 fedora-rawhide
msgid "print the data in sexps instead of rec format."
msgstr "mostra dados em sexps, em vez de formato rec."


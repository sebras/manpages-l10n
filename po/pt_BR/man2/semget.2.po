msgid "Program source"
msgstr "Fonte do programa"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-rawhide mageia-cauldron
#: opensuse-tumbleweed
#, no-wrap
msgid "/* t_semget.c\n"
msgstr "/* t_semget.c\n"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-rawhide mageia-cauldron
#: opensuse-tumbleweed
#, no-wrap
msgid ""
"   Licensed under GNU General Public License v2 or later.\n"
"*/\n"
"#include E<lt>sys/types.hE<gt>\n"
"#include E<lt>sys/ipc.hE<gt>\n"
"#include E<lt>sys/sem.hE<gt>\n"
"#include E<lt>sys/stat.hE<gt>\n"
"#include E<lt>stdio.hE<gt>\n"
"#include E<lt>stdlib.hE<gt>\n"
"#include E<lt>unistd.hE<gt>\n"
msgstr ""

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-rawhide mageia-cauldron
#: opensuse-tumbleweed
#, no-wrap
msgid ""
"static void\n"
"usage(const char *pname)\n"
"{\n"
"    fprintf(stderr, \"Usage: %s [-cx] pathname proj-id num-sems\\en\",\n"
"            pname);\n"
"    fprintf(stderr, \"    -c           Use IPC_CREAT flag\\en\");\n"
"    fprintf(stderr, \"    -x           Use IPC_EXCL flag\\en\");\n"
"    exit(EXIT_FAILURE);\n"
"}\n"
msgstr ""
"static void\n"
"usage(const char *pname)\n"
"{\n"
"    fprintf(stderr, \"Usage: %s [-cx] pathname proj-id num-sems\\en\",\n"
"            pname);\n"
"    fprintf(stderr, \"    -c           Use IPC_CREAT flag\\en\");\n"
"    fprintf(stderr, \"    -x           Use IPC_EXCL flag\\en\");\n"
"    exit(EXIT_FAILURE);\n"
"}\n"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-rawhide mageia-cauldron
#: opensuse-tumbleweed
#, no-wrap
msgid ""
"int\n"
"main(int argc, char *argv[])\n"
"{\n"
"    int semid, nsems, flags, opt;\n"
"    key_t key;\n"
msgstr ""
"int\n"
"main(int argc, char *argv[])\n"
"{\n"
"    int semid, nsems, flags, opt;\n"
"    key_t key;\n"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-rawhide mageia-cauldron
#: opensuse-tumbleweed
#, no-wrap
msgid ""
"    flags = 0;\n"
"    while ((opt = getopt(argc, argv, \"cx\")) != -1) {\n"
"        switch (opt) {\n"
"        case \\(aqc\\(aq: flags |= IPC_CREAT;   break;\n"
"        case \\(aqx\\(aq: flags |= IPC_EXCL;    break;\n"
"        default:  usage(argv[0]);\n"
"        }\n"
"    }\n"
msgstr ""
"    flags = 0;\n"
"    while ((opt = getopt(argc, argv, \"cx\")) != -1) {\n"
"        switch (opt) {\n"
"        case \\(aqc\\(aq: flags |= IPC_CREAT;   break;\n"
"        case \\(aqx\\(aq: flags |= IPC_EXCL;    break;\n"
"        default:  usage(argv[0]);\n"
"        }\n"
"    }\n"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-rawhide mageia-cauldron
#: opensuse-tumbleweed
#, no-wrap
msgid ""
"    if (argc != optind + 3)\n"
"        usage(argv[0]);\n"
msgstr ""
"    if (argc != optind + 3)\n"
"        usage(argv[0]);\n"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-rawhide mageia-cauldron
#: opensuse-tumbleweed
#, no-wrap
msgid ""
"    key = ftok(argv[optind], argv[optind + 1][0]);\n"
"    if (key == -1) {\n"
"        perror(\"ftok\");\n"
"        exit(EXIT_FAILURE);\n"
"    }\n"
msgstr ""
"    key = ftok(argv[optind], argv[optind + 1][0]);\n"
"    if (key == -1) {\n"
"        perror(\"ftok\");\n"
"        exit(EXIT_FAILURE);\n"
"    }\n"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-rawhide mageia-cauldron
#: opensuse-tumbleweed
#, no-wrap
msgid "    nsems = atoi(argv[optind + 2]);\n"
msgstr "    nsems = atoi(argv[optind + 2]);\n"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-rawhide mageia-cauldron
#: opensuse-tumbleweed
#, no-wrap
msgid ""
"    semid = semget(key, nsems, flags | 0600);\n"
"    if (semid == -1) {\n"
"        perror(\"semget\");\n"
"        exit(EXIT_FAILURE);\n"
"    }\n"
msgstr ""
"    semid = semget(key, nsems, flags | 0600);\n"
"    if (semid == -1) {\n"
"        perror(\"semget\");\n"
"        exit(EXIT_FAILURE);\n"
"    }\n"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-rawhide mageia-cauldron
#: opensuse-tumbleweed
#, no-wrap
msgid "    printf(\"ID = %d\\en\", semid);\n"
msgstr "    printf(\"ID = %d\\en\", semid);\n"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-rawhide mageia-cauldron
#: opensuse-tumbleweed
#, no-wrap
msgid ""
"    exit(EXIT_SUCCESS);\n"
"}\n"
msgstr ""
"    exit(EXIT_SUCCESS);\n"
"}\n"


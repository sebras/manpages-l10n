#| msgid "B<wchar_t *wmemcpy (wchar_t *>I<dest>B<, const wchar_t *>I<src>B<, size_t >I<n>B<);>\n"
msgid ""
"B<wchar_t *wmemcpy(wchar_t *restrict >I<dest>B<, const wchar_t *restrict >I<src>B<,>\n"

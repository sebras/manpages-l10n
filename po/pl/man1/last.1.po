#. type: TH
#: debian-bullseye
#, no-wrap
msgid "LAST, LASTB"
msgstr "LAST, LASTB"

#. type: TH
#: debian-bullseye
#, no-wrap
msgid "October 2013"
msgstr "październik 2013"

#. type: TH

#: opensuse-leap-15-4
#, no-wrap
msgid "/proc/meminfo"
msgstr "/proc/meminfo"

#. type: Plain text
#: opensuse-leap-15-4
msgid "memory information"
msgstr "informacje o pamięci"

#. type: SH
#: opensuse-leap-15-4
#, no-wrap
msgid "BUGS"

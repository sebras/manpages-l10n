msgid "  *.Z)\tuncompress -c $1 E<gt>/tmp/less.$$ 2E<gt>/dev/null"
msgstr "  *.Z)\tuncompress -c $1 E<gt>/tmp/less.$$ 2E<gt>/dev/null"


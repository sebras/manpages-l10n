"pnmtorast(1), pnmtotiff(1), pnmtoxwd(1), pnmarith(1), pnmcat(1), "
"pnmconvol(1), pnmcrop(1), pnmcut(1), pnmdepth(1), pnmenlarge(1), pnmfile(1), "
"pnmflip(1), pnmgamma(1), pnmindex(1), pnminvert(1), pnmmargin(1), "

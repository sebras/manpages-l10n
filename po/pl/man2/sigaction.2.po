"#include E<lt>stdlib.hE<gt>\n"
"#include E<lt>stdio.hE<gt>\n"
"#include E<lt>unistd.hE<gt>\n"
msgstr ""
"#include E<lt>signal.hE<gt>\n"
"#include E<lt>stdlib.hE<gt>\n"
"#include E<lt>stdio.hE<gt>\n"
"#include E<lt>unistd.hE<gt>\n"

#. type: Plain text
#: archlinux debian-unstable fedora-37 fedora-rawhide mageia-cauldron
#: opensuse-tumbleweed
#, no-wrap
msgid ""
"void\n"
"handler(int signo, siginfo_t *info, void *context)\n"
"{\n"
"    struct sigaction oldact;\n"
msgstr ""
"void\n"
"handler(int signo, siginfo_t *info, void *context)\n"
"{\n"
"    struct sigaction oldact;\n"

#. type: Plain text
#: archlinux debian-unstable fedora-37 fedora-rawhide mageia-cauldron
#: opensuse-tumbleweed
#, no-wrap
msgid ""
"    if (sigaction(SIGSEGV, NULL, &oldact) == -1 ||\n"
"            (oldact.sa_flags & SA_UNSUPPORTED) ||\n"
"            !(oldact.sa_flags & SA_EXPOSE_TAGBITS)) {\n"
"        _exit(EXIT_FAILURE);\n"
"    }\n"
"    _exit(EXIT_SUCCESS);\n"
"}\n"
msgstr ""
"    if (sigaction(SIGSEGV, NULL, &oldact) == -1 ||\n"
"            (oldact.sa_flags & SA_UNSUPPORTED) ||\n"
"            !(oldact.sa_flags & SA_EXPOSE_TAGBITS)) {\n"
"        _exit(EXIT_FAILURE);\n"
"    }\n"
"    _exit(EXIT_SUCCESS);\n"
"}\n"

#. type: Plain text
#: archlinux debian-unstable fedora-37 fedora-rawhide mageia-cauldron
#: opensuse-tumbleweed
#, no-wrap
msgid ""
"int\n"
"main(void)\n"
"{\n"
"    struct sigaction act = { 0 };\n"
msgstr ""
"int\n"
"main(void)\n"
"{\n"
"    struct sigaction act = { 0 };\n"

#. type: Plain text
#: archlinux debian-unstable fedora-37 fedora-rawhide mageia-cauldron
#: opensuse-tumbleweed
#, no-wrap
msgid ""
"    act.sa_flags = SA_SIGINFO | SA_UNSUPPORTED | SA_EXPOSE_TAGBITS;\n"
"    act.sa_sigaction = &handler;\n"
"    if (sigaction(SIGSEGV, &act, NULL) == -1) {\n"
"        perror(\"sigaction\");\n"
"        exit(EXIT_FAILURE);\n"
"    }\n"
msgstr ""
"    act.sa_flags = SA_SIGINFO | SA_UNSUPPORTED | SA_EXPOSE_TAGBITS;\n"
"    act.sa_sigaction = &handler;\n"
"    if (sigaction(SIGSEGV, &act, NULL) == -1) {\n"
"        perror(\"sigaction\");\n"
"        exit(EXIT_FAILURE);\n"
"    }\n"

#. type: Plain text
#: archlinux debian-unstable fedora-37 fedora-rawhide mageia-cauldron
#: opensuse-tumbleweed
#, no-wrap
msgid ""
"    raise(SIGSEGV);\n"
"}\n"
msgstr ""
"    raise(SIGSEGV);\n"
"}\n"


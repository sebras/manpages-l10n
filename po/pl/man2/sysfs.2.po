#, fuzzy, no-wrap
#| msgid "B<int sysfs(int >I<option>B<, unsigned int >I<fs_index>B<, char *>I<buf>B<);>"
msgid ""
"B<int sysfs(int >I<option>B<, const char *>I<fsname>B<);>\n"
"B<int sysfs(int >I<option>B<, unsigned int >I<fs_index>B<, char *>I<buf>B<);>\n"
"B<int sysfs(int >I<option>B<);>\n"
msgstr "B<int sysfs(int >I<option>B<, unsigned int >I<fs_index>B<, char *>I<buf>B<);>"

#. type: SH
#: archlinux debian-bullseye debian-unstable fedora-rawhide mageia-cauldron

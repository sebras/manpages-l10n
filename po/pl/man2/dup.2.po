#. type: Plain text
#: archlinux debian-unstable fedora-37 fedora-rawhide mageia-cauldron
#: opensuse-tumbleweed
#, no-wrap
msgid ""
"if (tmpfd != -1) {\n"
"    if (close(tmpfd) == -1) {\n"
"        /* Handle errors from close. */\n"
"    }\n"
"}\n"
msgstr ""


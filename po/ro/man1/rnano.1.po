#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
#, no-wrap
msgid "\\[bu]"
msgstr "\\[bu]"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
msgid "not allow suspending;"
msgstr "nu va permite suspendarea;"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
msgid "not allow saving the current buffer under a different name;"
msgstr "nu va permite salvarea buffer-ului curent sub un alt nume;"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
msgid "not allow inserting another file or opening a new buffer;"
msgstr ""
"nu va permite introducerea altui fișier sau deschiderea unui nou buffer;"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
msgid "not allow appending or prepending to any file;"
msgstr "nu va permite completarea sau inserarea la început la orice fișier;"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-36 fedora-rawhide
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
msgid "not make backup files nor do spell checking."
msgstr "nu va face fișiere de rezervă și nici nu va verifica ortografia."


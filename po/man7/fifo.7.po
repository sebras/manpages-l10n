msgid "FIFO"
msgstr "FIFO"

#. type: TH
#, no-wrap
msgid "2008-12-03"
msgstr "3. Dezember 2008"

#. type: TH
#, no-wrap
msgid "Linux"
msgstr "Linux"

#. type: TH

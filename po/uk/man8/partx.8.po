#: debian-bullseye
#, no-wrap
#| msgid "B<-P>, B<--pairs>"
msgid "B<-P>,B< --pairs>"
msgstr "B<-P>, B<--pairs>"

#. type: TP
#: debian-bullseye
#, no-wrap
msgid "B<-r>,B< --raw>"
msgstr "B<-r>,B< --raw>"


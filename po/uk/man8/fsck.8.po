#. type: Plain text
#. #-#-#-#-#  mageia-cauldron: fsck.8.pot (PACKAGE VERSION)  #-#-#-#-#
#. type: Plain text
#. #-#-#-#-#  opensuse-leap-15-5: fsck.8.pot (PACKAGE VERSION)  #-#-#-#-#
#. type: Plain text

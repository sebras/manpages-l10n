#. type: Plain text
#. #-#-#-#-#  mageia-cauldron: ionice.1.pot (PACKAGE VERSION)  #-#-#-#-#
#. type: Plain text
#. #-#-#-#-#  opensuse-leap-15-5: ionice.1.pot (PACKAGE VERSION)  #-#-#-#-#
#. type: Plain text

#, no-wrap
msgid ""
"    printf(\"ID of containing device:  [%jx,%jx]\\en\",\n"
"            (uintmax_t) major(sb.st_dev),\n"
"            (uintmax_t) minor(sb.st_dev));\n"
msgstr ""
"    printf(\"ID van het bevattende apparaat:  [%jx,%jx]\\en\",\n"

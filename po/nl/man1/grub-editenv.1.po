#, no-wrap
msgid "GRUB-EDITENV"
msgstr "GRUB-EDITENV"

#. type: TH
#: archlinux
#, fuzzy, no-wrap
#| msgid "October 2021"
msgid "October 2022"
msgstr "Oktober 2021"

#. type: TH
#: archlinux
#, fuzzy, no-wrap
#| msgid "GRUB 2:2.06.r261.g2f4430cc0-1"
msgid "GRUB 2:2.06.r334.g340377470-1"

msgstr "B<-e>"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-rawhide mageia-cauldron
#: opensuse-leap-15-3 opensuse-tumbleweed
msgid "equivalent to B<-vE>"
msgstr "hetzelfde als B<-vE>"

#. type: TP
#: archlinux debian-bullseye debian-unstable fedora-rawhide mageia-cauldron
#: opensuse-leap-15-3 opensuse-tumbleweed
#, no-wrap
msgid "B<-E>, B<--show-ends>"
msgstr "B<-E>, B<--show-ends>"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-rawhide mageia-cauldron
#: opensuse-leap-15-3 opensuse-tumbleweed
msgid "display $ at end of each line"
msgstr "een $ weergeven aan het einde van elke regel"

#. type: TP
#: archlinux debian-bullseye debian-unstable fedora-rawhide mageia-cauldron
#: opensuse-leap-15-3 opensuse-tumbleweed
#, no-wrap
msgid "B<-n>, B<--number>"
msgstr "B<-n>, B<--number>"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-rawhide mageia-cauldron
#: opensuse-leap-15-3 opensuse-tumbleweed
msgid "number all output lines"
msgstr "alle uitvoerregels nummeren"

#. type: TP
#: archlinux debian-bullseye debian-unstable fedora-rawhide mageia-cauldron
#: opensuse-leap-15-3 opensuse-tumbleweed
#, no-wrap
msgid "B<-s>, B<--squeeze-blank>"
msgstr "B<-s>, B<--squeeze-blank>"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-rawhide mageia-cauldron
#: opensuse-leap-15-3 opensuse-tumbleweed
msgid "suppress repeated empty output lines"
msgstr "opeenvolgende blanco regels comprimeren tot één"

#. type: TP
#: archlinux debian-bullseye debian-unstable fedora-rawhide mageia-cauldron
#: opensuse-leap-15-3 opensuse-tumbleweed
#, no-wrap
msgid "B<-t>"
msgstr "B<-t>"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-rawhide mageia-cauldron
#: opensuse-leap-15-3 opensuse-tumbleweed
msgid "equivalent to B<-vT>"
msgstr "hetzelfde als B<-vT>"


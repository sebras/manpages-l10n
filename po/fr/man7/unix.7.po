msgid ""
"    /*\n"
"     * In case the program exited inadvertently on the last run,\n"
"     * remove the socket.\n"
"     */\n"
msgstr ""
"    /*\n"
"     * Dans le cas où le programme quitte accidentellement lors\n"
"     * de la dernière exécution, supprimer le socket.\n"
"     */\n"

#. type: Plain text
#: opensuse-leap-15-4
#, no-wrap
msgid "    memset(&name, 0, sizeof(struct sockaddr_un));\n"
msgstr "    memset(&name, 0, sizeof(struct sockaddr_un));\n"

#. type: Plain text
#: opensuse-leap-15-4
#, no-wrap
msgid ""
"    ret = bind(connection_socket, (const struct sockaddr *) &name,\n"
"               sizeof(struct sockaddr_un));\n"
"    if (ret == -1) {\n"
"        perror(\"bind\");\n"
"        exit(EXIT_FAILURE);\n"
"    }\n"
msgstr ""
"    ret = bind(connection_socket, (const struct sockaddr *) &name,\n"
"               sizeof(struct sockaddr_un));\n"
"    if (ret == -1) {\n"
"        perror(\"bind\");\n"
"        exit(EXIT_FAILURE);\n"
"    }\n"

#. type: Plain text
#: opensuse-leap-15-4
#, no-wrap
msgid ""
"        result = 0;\n"
"        for(;;) {\n"
msgstr ""
"        result = 0;\n"
"        for(;;) {\n"

#. type: Plain text
#: opensuse-leap-15-4
#, no-wrap
msgid ""
"            ret = read(data_socket, buffer, BUFFER_SIZE);\n"
"            if (ret == -1) {\n"
"                perror(\"read\");\n"
"                exit(EXIT_FAILURE);\n"
"            }\n"
msgstr ""
"            ret = read(data_socket, buffer, BUFFER_SIZE);\n"
"            if (ret == -1) {\n"
"                perror(\"read\");\n"
"                exit(EXIT_FAILURE);\n"
"            }\n"

#. type: Plain text
#: opensuse-leap-15-4
#, no-wrap
msgid "            buffer[BUFFER_SIZE - 1] = 0;\n"
msgstr "            buffer[BUFFER_SIZE - 1] = 0;\n"

#. type: Plain text
#: opensuse-leap-15-4
#, no-wrap
msgid ""
"            if (!strncmp(buffer, \"DOWN\", BUFFER_SIZE)) {\n"
"                down_flag = 1;\n"
"                break;\n"
"            }\n"
msgstr ""
"            if (!strncmp(buffer, \"DOWN\", BUFFER_SIZE)) {\n"
"                down_flag = 1;\n"
"                break;\n"
"            }\n"

#. type: Plain text
#: opensuse-leap-15-4
#, no-wrap
msgid ""
"            if (!strncmp(buffer, \"END\", BUFFER_SIZE)) {\n"
"                break;\n"
"            }\n"
msgstr ""
"            if (!strncmp(buffer, \"END\", BUFFER_SIZE)) {\n"
"                break;\n"
"            }\n"

#. type: Plain text
#: opensuse-leap-15-4
#, no-wrap
msgid ""
"        sprintf(buffer, \"%d\", result);\n"
"        ret = write(data_socket, buffer, BUFFER_SIZE);\n"
msgstr ""
"        sprintf(buffer, \"%d\", result);\n"
"        ret = write(data_socket, buffer, BUFFER_SIZE);\n"

#. type: Plain text
#: opensuse-leap-15-4
#, no-wrap
msgid ""
"        if (ret == -1) {\n"
"            perror(\"write\");\n"
"            exit(EXIT_FAILURE);\n"
"        }\n"
msgstr ""
"        if (ret == -1) {\n"
"            perror(\"write\");\n"
"            exit(EXIT_FAILURE);\n"
"        }\n"

#. type: Plain text
#: opensuse-leap-15-4
#, no-wrap
msgid ""
"int\n"
"main(int argc, char *argv[])\n"
"{\n"
"    struct sockaddr_un addr;\n"
"    int i;\n"
"    int ret;\n"
"    int data_socket;\n"
"    char buffer[BUFFER_SIZE];\n"
msgstr ""
"int\n"
"main(int argc, char *argv[])\n"
"{\n"
"    struct sockaddr_un addr;\n"
"    int i;\n"
"    int ret;\n"
"    int data_socket;\n"
"    char buffer[BUFFER_SIZE];\n"

#. type: Plain text
#: opensuse-leap-15-4
#, no-wrap
msgid "    memset(&addr, 0, sizeof(struct sockaddr_un));\n"
msgstr "    memset(&addr, 0, sizeof(struct sockaddr_un));\n"

#. type: Plain text
#: opensuse-leap-15-4
#, no-wrap
msgid ""
"    ret = connect (data_socket, (const struct sockaddr *) &addr,\n"
"                   sizeof(struct sockaddr_un));\n"
"    if (ret == -1) {\n"
"        fprintf(stderr, \"The server is down.\\en\");\n"
"        exit(EXIT_FAILURE);\n"
"    }\n"
msgstr ""
"    ret = connect (data_socket, (const struct sockaddr *) &addr,\n"
"                   sizeof(struct sockaddr_un));\n"
"    if (ret == -1) {\n"
"        fprintf(stderr, \"Le serveur est arrêté.\\en\");\n"
"        exit(EXIT_FAILURE);\n"
"    }\n"

#. type: Plain text
#: opensuse-leap-15-4
#, no-wrap
msgid ""
"    for (i = 1; i E<lt> argc; ++i) {\n"
"        ret = write(data_socket, argv[i], strlen(argv[i]) + 1);\n"
"        if (ret == -1) {\n"
"            perror(\"write\");\n"
"            break;\n"
"        }\n"
"    }\n"
msgstr ""
"    for (i = 1; i E<lt> argc; ++i) {\n"
"        ret = write(data_socket, argv[i], strlen(argv[i]) + 1);\n"
"        if (ret == -1) {\n"
"            perror(\"write\");\n"
"            break;\n"
"        }\n"
"    }\n"

#. type: Plain text
#: opensuse-leap-15-4
#, no-wrap
msgid ""
"    strcpy (buffer, \"END\");\n"
"    ret = write(data_socket, buffer, strlen(buffer) + 1);\n"
"    if (ret == -1) {\n"
"        perror(\"write\");\n"
"        exit(EXIT_FAILURE);\n"
"    }\n"
msgstr ""
"    strcpy (buffer, \"END\");\n"
"    ret = write(data_socket, buffer, strlen(buffer) + 1);\n"
"    if (ret == -1) {\n"
"        perror(\"write\");\n"
"        exit(EXIT_FAILURE);\n"
"    }\n"

#. type: Plain text
#: opensuse-leap-15-4
#, no-wrap
msgid ""
"    ret = read(data_socket, buffer, BUFFER_SIZE);\n"
"    if (ret == -1) {\n"
"        perror(\"read\");\n"
"        exit(EXIT_FAILURE);\n"
"    }\n"
msgstr ""
"    ret = read(data_socket, buffer, BUFFER_SIZE);\n"
"    if (ret == -1) {\n"
"        perror(\"read\");\n"
"        exit(EXIT_FAILURE);\n"
"    }\n"

#. type: Plain text
#: opensuse-leap-15-4
#, no-wrap
msgid "    buffer[BUFFER_SIZE - 1] = 0;\n"
msgstr "    buffer[BUFFER_SIZE - 1] = 0;\n"


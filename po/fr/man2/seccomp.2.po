#. type: Plain text
#: opensuse-leap-15-5
#, no-wrap
msgid ""
"$ B<uname -m>\n"
"x86_64\n"
"$ B<syscall_nr() {\n"
"    cat /usr/src/linux/arch/x86/syscalls/syscall_64.tbl | \\e\n"
"    awk '$2 != \"x32\" && $3 == \"'$1'\" { print $1 }'\n"
"}>\n"
msgstr ""
"$ B<uname -m>\n"
"x86_64\n"
"$ B<syscall_nr() {\n"
"    cat /usr/src/linux/arch/x86/syscalls/syscall_64.tbl | \\e\n"
"    awk '$2 != \"x32\" && $3 == \"'$1'\" { print $1 }'\n"
"}>\n"

#. type: Plain text
#: opensuse-leap-15-5
#, no-wrap
msgid "#define X32_SYSCALL_BIT 0x40000000\n"
msgstr "#define X32_SYSCALL_BIT 0x40000000\n"

#. type: Plain text
#: opensuse-leap-15-5
#, no-wrap
msgid ""
"    /* Assume that AUDIT_ARCH_X86_64 means the normal x86-64 ABI\n"
"       (in the x32 ABI, all system calls have bit 30 set in the\n"
"       'nr' field, meaning the numbers are E<gt>= X32_SYSCALL_BIT) */\n"
"    if (t_arch == AUDIT_ARCH_X86_64)\n"
"        upper_nr_limit = X32_SYSCALL_BIT - 1;\n"
msgstr ""
"    /* Supposer qu'AUDIT_ARCH_X86_64 signifie l'ABI x86-64 normale\n"
"       (dans l'ABI x32, tous les appels système ont positionné 30 bits\n"
"       dans le champ « nr », ce qui veut dire que les numéros\n"
"       sont E<gt>= X32_SYSCALL_BIT) */\n"
"    if (t_arch == AUDIT_ARCH_X86_64)\n"
"        upper_nr_limit = X32_SYSCALL_BIT - 1;\n"

#. type: Plain text
#: opensuse-leap-15-5
#, no-wrap
msgid ""
"    struct sock_filter filter[] = {\n"
"        /* [0] Load architecture from 'seccomp_data' buffer into\n"
"               accumulator */\n"
"        BPF_STMT(BPF_LD | BPF_W | BPF_ABS,\n"
"                 (offsetof(struct seccomp_data, arch))),\n"
msgstr ""
"    struct sock_filter filter[] = {\n"
"        /* [0] Charger l'architecture depuis le tampon « seccomp_data »\n"
"               dans l'accumulateur */\n"
"        BPF_STMT(BPF_LD | BPF_W | BPF_ABS,\n"
"                 (offsetof(struct seccomp_data, arch))),\n"

#. type: Plain text
#: opensuse-leap-15-5
#, no-wrap
msgid ""
"        /* [1] Jump forward 5 instructions if architecture does not\n"
"               match 't_arch' */\n"
"        BPF_JUMP(BPF_JMP | BPF_JEQ | BPF_K, t_arch, 0, 5),\n"
msgstr ""
"        /* [1] Revenir de 5 instructions si l'architecture ne correspond\n"
"               pas à « t_arch » */\n"
"        BPF_JUMP(BPF_JMP | BPF_JEQ | BPF_K, t_arch, 0, 5),\n"

#. type: Plain text
#: opensuse-leap-15-5
#, no-wrap
msgid ""
"        /* [2] Load system call number from 'seccomp_data' buffer into\n"
"               accumulator */\n"
"        BPF_STMT(BPF_LD | BPF_W | BPF_ABS,\n"
"                 (offsetof(struct seccomp_data, nr))),\n"
msgstr ""
"        /* [2] Charger le numéro d'appel système depuis le tampon\n"
"               « seccomp_data » dans l'accumulateur */\n"
"        BPF_STMT(BPF_LD | BPF_W | BPF_ABS,\n"
"                 (offsetof(struct seccomp_data, nr))),\n"

#. type: Plain text
#: opensuse-leap-15-5
#, no-wrap
msgid ""
"        /* [3] Check ABI - only needed for x86-64 in blacklist use\n"
"               cases.  Use BPF_JGT instead of checking against the bit\n"
"               mask to avoid having to reload the syscall number. */\n"
"        BPF_JUMP(BPF_JMP | BPF_JGT | BPF_K, upper_nr_limit, 3, 0),\n"
msgstr ""
"        /* [3] Vérifier l'ABI - uniquement nécessaire pour x86-64 si on\n"
"               utilise une liste noire. Utilisez BPF_JGT au lieu de vérifier\n"
"               par rapport au masque de bit pour ne pas devoir recharger\n"
"               le numéro d'appel système. */\n"
"        BPF_JUMP(BPF_JMP | BPF_JGT | BPF_K, upper_nr_limit, 3, 0),\n"

#. type: Plain text
#: opensuse-leap-15-5
#, no-wrap
msgid ""
"        /* [4] Jump forward 1 instruction if system call number\n"
"               does not match 'syscall_nr' */\n"
"        BPF_JUMP(BPF_JMP | BPF_JEQ | BPF_K, syscall_nr, 0, 1),\n"
msgstr ""
"        /* [4] Revenir d'une instruction si le numéro d'appel système\n"
"               ne correspond pas à « syscall_nr » */\n"
"        BPF_JUMP(BPF_JMP | BPF_JEQ | BPF_K, syscall_nr, 0, 1),\n"

#. type: Plain text
#: opensuse-leap-15-5
#, no-wrap
msgid ""
"        /* [5] Matching architecture and system call: don't execute\n"
"\t       the system call, and return 'f_errno' in 'errno' */\n"
"        BPF_STMT(BPF_RET | BPF_K,\n"
"                 SECCOMP_RET_ERRNO | (f_errno & SECCOMP_RET_DATA)),\n"
msgstr ""
"        /* [5] Architecture et numéro d'appel système trouvés : ne pas\n"
"\t       exécuter l'appel système et renvoyer « f_errno » dans « errno » */\n"
"        BPF_STMT(BPF_RET | BPF_K,\n"
"                 SECCOMP_RET_ERRNO | (f_errno & SECCOMP_RET_DATA)),\n"

#. type: Plain text
#: opensuse-leap-15-5
#, no-wrap
msgid ""
"        /* [7] Destination of architecture mismatch: kill task */\n"
"        BPF_STMT(BPF_RET | BPF_K, SECCOMP_RET_KILL),\n"
"    };\n"
msgstr ""
"        /* [7] Cible de l'architecture inadéquate : tuer la tâche */\n"
"        BPF_STMT(BPF_RET | BPF_K, SECCOMP_RET_KILL),\n"
"    };\n"

#. type: Plain text
#: opensuse-leap-15-5
#, no-wrap
msgid ""
"    struct sock_fprog prog = {\n"
"        .len = (unsigned short) (sizeof(filter) / sizeof(filter[0])),\n"
"        .filter = filter,\n"
"    };\n"
msgstr ""
"    struct sock_fprog prog = {\n"
"        .len = (unsigned short) (sizeof(filter) / sizeof(filter[0])),\n"
"        .filter = filter,\n"
"    };\n"


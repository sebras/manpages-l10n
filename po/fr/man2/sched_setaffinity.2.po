msgid ""
"$ B<lscpu | grep -i 'core.*:|socket'>\n"
"Thread(s) per core:    2\n"
"Core(s) per socket:    2\n"
"Socket(s):             1\n"
msgstr ""
"$ B<lscpu | grep -i 'core.*:|socket'>\n"
"Thread(s) par cœur :    2\n"
"Cœur(s) par socket :    2\n"
"Socket(s) :             1\n"


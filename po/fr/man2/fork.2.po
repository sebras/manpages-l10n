msgid ""
"int\n"
"main(void)\n"
"{\n"
"    pid_t pid;\n"
msgstr ""

#. type: Plain text
#: archlinux
#, no-wrap
msgid ""
"    if (signal(SIGCHLD, SIG_IGN) == SIG_ERR) {\n"
"        perror(\"signal\");\n"
"        exit(EXIT_FAILURE);\n"
"    }\n"
"    pid = fork();\n"
"    switch (pid) {\n"
"    case -1:\n"
"        perror(\"fork\");\n"
"        exit(EXIT_FAILURE);\n"
"    case 0:\n"
"        puts(\"Child exiting.\");\n"
"        exit(EXIT_SUCCESS);\n"
"    default:\n"
"        printf(\"Child is PID %jd\\en\", (intmax_t) pid);\n"
"        puts(\"Parent exiting.\");\n"
"        exit(EXIT_SUCCESS);\n"
"    }\n"
"}\n"
msgstr ""


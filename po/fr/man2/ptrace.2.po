"    __u8 op;        /* Type of system call stop */\n"
"    __u32 arch;     /* AUDIT_ARCH_* value; see seccomp(2) */\n"
"    __u64 instruction_pointer; /* CPU instruction pointer */\n"
"    __u64 stack_pointer;    /* CPU stack pointer */\n"

"        } exit;\n"
"        struct {    /* op == PTRACE_SYSCALL_INFO_SECCOMP */\n"
"            __u64 nr;       /* System call number */\n"
"            __u64 args[6];  /* System call arguments */\n"
"            __u32 ret_data; /* SECCOMP_RET_DATA portion\n"

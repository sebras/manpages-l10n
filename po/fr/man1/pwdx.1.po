#. type: TH
#: opensuse-leap-15-4
#, no-wrap
msgid "PWDX"
msgstr "PWDX"

#. type: TH
#: opensuse-leap-15-4
#, no-wrap
msgid "June 2011"
msgstr "Juin 2011"

#. type: TH
#: opensuse-leap-15-4
#, no-wrap
msgid "procps-ng"
msgstr "procps-ng"

#. type: TH
#: opensuse-leap-15-4
#, no-wrap
msgid "User Commands"
msgstr "Commandes de l'utilisateur"

#. type: SH
#: opensuse-leap-15-4
#, no-wrap
msgid "NAME"
msgstr "NOM"

#. type: Plain text
#: opensuse-leap-15-4
msgid "pwdx - report current working directory of a process"
msgstr "pwdx - Afficher le répertoire de travail d'un processus"

#. type: SH
#: opensuse-leap-15-4
#, no-wrap
msgid "SYNOPSIS"
msgstr "SYNOPSIS"

#. type: Plain text
#: opensuse-leap-15-4
msgid "B<pwdx> [I<options>] I<pid> [...]"
msgstr "B<pwdx> [I<options>] I<PID> [...]"

#. type: SH
#: opensuse-leap-15-4
#, no-wrap
msgid "OPTIONS"
msgstr "OPTIONS"

#. type: TP
#: opensuse-leap-15-4
#, no-wrap
msgid "B<-V>, B<--version>"
msgstr "B<-V>, B<--version>"

#. type: Plain text
#: opensuse-leap-15-4
msgid "Output version information and exit."
msgstr "Afficher le numéro de version et quitter."

#. type: TP
#: opensuse-leap-15-4
#, no-wrap
msgid "B<-h>, B<--help>"
msgstr "B<-h>, B<--help>"

#. type: Plain text
#: opensuse-leap-15-4
msgid "Output help screen and exit."
msgstr "Afficher un écran d'aide puis quitter."

#. type: SH
#: opensuse-leap-15-4
#, no-wrap
msgid "SEE ALSO"
msgstr "VOIR AUSSI"

#. type: Plain text
#: opensuse-leap-15-4
msgid "B<ps>(1), B<pgrep>(1)"
msgstr "B<ps>(1), B<pgrep>(1)"

#. type: SH
#: opensuse-leap-15-4
#, no-wrap
msgid "STANDARDS"
msgstr "STANDARDS"

#. type: Plain text
#: opensuse-leap-15-4
#, fuzzy
#| msgid ""
#| "No standards apply, but pwdx looks an awful lot like a SunOS command."
msgid "No standards apply, but pwdx looks an awful lot like a SunOS command."
msgstr ""
"Aucune norme n'est respectée mais B<pwdx> ressemble fortement à une commande "
"SunOS."

#. type: SH
#: opensuse-leap-15-4
#, no-wrap
msgid "AUTHOR"
msgstr "AUTEUR"

#. type: Plain text
#: opensuse-leap-15-4
msgid "E<.UR nmiell@gmail.com> Nicholas Miell E<.UE> wrote pwdx in 2004."
msgstr "Nicholas Miell E<lt>I<nmiell@gmail.com>E<gt> a écrit pwdx en 2004."

#. type: SH
#: opensuse-leap-15-4
#, no-wrap
msgid "REPORTING BUGS"
msgstr "SIGNALER DES BOGUES"

#. type: Plain text
#: opensuse-leap-15-4
msgid "Please send bug reports to E<.UR procps@freelists.org> E<.UE>"
msgstr ""
"Merci d'envoyer un rapport de bogue à E<.UR procps@freelists.org> E<.UE>"

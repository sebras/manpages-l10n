msgid ""
"  4a.I< Global-Commands >\n"
"        E<lt>Ent/SpE<gt> ?, =, 0,\n"
"        A, B, d, E, e, g, h, H, I, k, q, r, s, W, X, Y, Z\n"
"  4b.I< Summary-Area-Commands >\n"
"        C, l, t, m, 1, 2, 3\n"
"  4c.I< Task-Area-Commands >\n"

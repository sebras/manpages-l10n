msgid "TLOAD"
msgstr "TLOAD"

#. type: TH
#: opensuse-leap-15-4
#, no-wrap
msgid "June 2011"
msgstr "Juin 2011"

#. type: TH
#: opensuse-leap-15-4
#, no-wrap
msgid "procps-ng"

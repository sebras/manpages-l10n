#| msgid ""
#| "B<intmax_t strtoimax(const char *>I<nptr>B<, char **>I<endptr>B<, int >I<base>B<);>\n"
#| "B<uintmax_t strtoumax(const char *>I<nptr>B<, char **>I<endptr>B<, int >I<base>B<);>\n"
msgid ""
"B<intmax_t strtoimax(const char *restrict >I<nptr>B<, char **restrict >I<endptr>B<,>\n"
"B<                   int >I<base>B<);>\n"
"B<uintmax_t strtoumax(const char *restrict >I<nptr>B<, char **restrict >I<endptr>B<,>\n"
"B<                   int >I<base>B<);>\n"

msgstr ""
"int\n"
"main(int argc, char *argv[])\n"
"{\n"
"#define LIM 4000000\n"
"    int j;\n"
"    char p[LIM + 1];    /* + 1 pour l'octet NULL final */\n"
"    time_t base;\n"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-rawhide mageia-cauldron
#: opensuse-leap-15-3 opensuse-tumbleweed
#, no-wrap
msgid ""
"    base = time(NULL);\n"
"    p[0] = \\(aq\\e0\\(aq;\n"
msgstr ""
"    base = time(NULL);\n"
"    p[0] = \\(aq\\e0\\(aq;\n"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-rawhide mageia-cauldron
#: opensuse-tumbleweed
#, fuzzy, no-wrap
#| msgid ""
#| "    for (j = 0; j E<lt> LIM; j++) {\n"
#| "        if ((j % 10000) == 0)\n"
#| "            printf(\"%d %ld\\en\", j, (long) (time(NULL) - base));\n"
#| "        strcat(p, \"a\");\n"
#| "    }\n"
#| "}\n"
msgid ""
"    for (int j = 0; j E<lt> LIM; j++) {\n"
"        if ((j % 10000) == 0)\n"
"            printf(\"%d %jd\\en\", j, (intmax_t) (time(NULL) - base));\n"
"        strcat(p, \"a\");\n"
"    }\n"
"}\n"
msgstr ""
"    for (j = 0; j E<lt> LIM; j++) {\n"
"        if ((j % 10000) == 0)\n"
"            printf(\"%d %ld\\en\", j, (long) (time(NULL) - base));\n"
"        strcat(p, \"a\");\n"
"    }\n"
"}\n"


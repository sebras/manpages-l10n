msgstr "B<blkid>"

#. type: Plain text
#: debian-bullseye
msgid "B<--label>I< label> | B<--uuid>I< uuid>"
msgstr "B<--label> I<étiquette> | B<--uuid> I<UUID>"

#. type: Plain text
#: debian-bullseye
msgid ""
"[B<--no-encoding> B<--garbage-collect --list-one --cache-file> I<file>] [B<--"
"output> I<format>] [B<--match-tag> I<tag>] [B<--match-token> I<NAME=value>] "
"[I<device> ...]"
msgstr ""
"[B<--no-encoding> B<--garbage-collect --list-one --cache-file> I<fichier>] "
"[B<--output> I<format>] [B<--match-tag> I<indicateur>] [B<--match-token> "
"I<NOM>B<=>I<valeur>] [I<périphérique> ...]"

#. type: Plain text
#: debian-bullseye
msgid ""
"B<--probe> [B<--offset> I<offset>] [B<--output> I<format>] [B<--size> "
"I<size>] [B<--match-tag> I<tag>] [B<--match-types> I<list>] [B<--usages> "

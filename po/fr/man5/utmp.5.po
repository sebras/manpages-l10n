"    int32_t ut_session;           /* ID de session  (B<getsid>(2)),\n"
"                                     utilisé pour le fenêtrage */\n"
"    struct {\n"
"        int32_t tv_sec;           /* Secondes */\n"
"        int32_t tv_usec;          /* Microsecondes */\n"
"    } ut_tv;                      /* Horodatage */\n"
"#else\n"
"    long int ut_session;          /* ID de session */\n"
"    struct timeval ut_tv;         /* Horodatage */\n"
"#endif\n"


#, no-wrap
msgid "GRUB-GLUE-EFI"
msgstr "GRUB-GLUE-EFI"

#. type: TH
#: mageia-cauldron
#, no-wrap
msgid "Wed Feb 26 2014"
msgstr "Thứ 4 ngày 26 tháng 2 năm 2014"

#. type: SH
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
#, no-wrap
msgid "NAME"
msgstr "TÊN"

#. type: Plain text
#: mageia-cauldron
msgid "B<grub-glue-efi> \\(em Create an Apple fat EFI binary."
msgstr ""

#. type: SH
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
#, no-wrap
msgid "SYNOPSIS"
msgstr "TÓM TẮT"

#. type: Plain text
#: mageia-cauldron
msgid ""
"B<grub-glue-efi> E<lt>-3 | --input32=I<FILE>E<gt> E<lt>-6 | --"
"input64=I<FILE>E<gt>"
msgstr ""
"B<grub-glue-efi> E<lt>-3 | --input32=I<TẬP_TIN>E<gt> E<lt>-6 | --"
"input64=I<TẬP_TIN>E<gt>"

#. type: Plain text
#: mageia-cauldron
msgid "E<lt>-o | --output=I<FILE>E<gt> [-v | --verbose]"
msgstr "E<lt>-o | --output=I<TẬP_TIN>E<gt> [-v | --verbose]"

#. type: SH
#: mageia-cauldron opensuse-leap-15-4 opensuse-tumbleweed
#, no-wrap
msgid "DESCRIPTION"
msgstr "MÔ TẢ"

#. type: Plain text
#: mageia-cauldron
msgid "B<grub-glue-efi> creates an Apple fat EFI binary from two EFI binaries."
msgstr ""

#. type: SH
#: mageia-cauldron
#, no-wrap
msgid "OPTIONS"
msgstr "TÙY CHỌN"

#. type: TP
#: mageia-cauldron
#, no-wrap

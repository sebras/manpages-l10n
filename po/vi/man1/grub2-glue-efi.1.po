#: mageia-cauldron
#, fuzzy
#| msgid "Read 32-bit value from PORT."
msgid "Read 32-bit binary from I<FILE>."
msgstr "Đọc giá trị 32-bit từ CỔNG."

#. type: TP
#: mageia-cauldron
#, no-wrap
msgid "B<--input64>=I<FILE>"
msgstr "B<--input64>=I<TẬP_TIN>"

#. type: Plain text
#: mageia-cauldron
#, fuzzy
#| msgid "Read 16-bit value from PORT."
msgid "Read 64-bit binary from I<FILE>."
msgstr "Đọc giá trị 16-bit từ CỔNG."

#. type: TP
#: mageia-cauldron
#, no-wrap
msgid "B<--output>=I<FILE>"
msgstr "B<--output>=I<TẬP_TIN>"

#. type: Plain text
#: mageia-cauldron
msgid "Write resulting fat binary to I<FILE>."
msgstr ""

#. type: TP
#: mageia-cauldron
#, no-wrap
msgid "B<--verbose>"
msgstr "B<--verbose>"

#. type: Plain text
#: mageia-cauldron
msgid "Print verbose messages."
msgstr "Hiển thị thông tin chi tiết."

